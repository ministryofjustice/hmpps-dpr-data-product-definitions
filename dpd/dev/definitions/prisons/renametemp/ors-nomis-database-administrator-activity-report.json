      "description": "C-NOMIS Audit",
      "datasource": "nomis",
      "query": "dataset_base_ AS (SELECT DISTINCT COALESCE(AUDITDATA.REMOTE_AUDIT_TRAIL.USERNAME, 'N/A') AS RAT_USERNAME, TRUNC(AUDITDATA.REMOTE_AUDIT_TRAIL.TIMESTAMP) AS DATE_OF_ACTION_, TO_CHAR(AUDITDATA.REMOTE_AUDIT_TRAIL.TIMESTAMP, 'HH24:MI:SS') AS TIME_OF_ACTION_, COALESCE(AUDITDATA.REMOTE_AUDIT_TRAIL.OS_USERNAME, 'N/A') AS RAT_OS_LOGIN_NAME, COALESCE(AUDITDATA.REMOTE_AUDIT_TRAIL.USERHOST, 'N/A') AS RAT_CLIENT_HOST_NAME, CASE WHEN AUDITDATA.REMOTE_AUDIT_TRAIL.TERMINAL LIKE 'UNKNO%' THEN 'N/A' WHEN AUDITDATA.REMOTE_AUDIT_TRAIL.TERMINAL IS NULL THEN 'N/A' ELSE AUDITDATA.REMOTE_AUDIT_TRAIL.TERMINAL END AS RAT_TERMINAL_IDENTIFIER, COALESCE(AUDITDATA.REMOTE_AUDIT_TRAIL.ACTION_NAME, 'N/A') AS RAT_ACTION_TYPE, AUDITDATA.REMOTE_AUDIT_TRAIL.RETURNCODE, COALESCE(AUDITDATA.REMOTE_AUDIT_TRAIL.OWNER, 'N/A') AS RAT_OBJECT_OWNER, COALESCE(AUDITDATA.REMOTE_AUDIT_TRAIL.OBJ_NAME, 'N/A') AS RAT_OBJECT_NAME, COALESCE(AUDITDATA.REMOTE_AUDIT_TRAIL.NEW_OWNER, 'N/A') AS RAT_NEW_OBJECT_OWNER, COALESCE(AUDITDATA.REMOTE_AUDIT_TRAIL.NEW_NAME, 'N/A') AS RAT_NEW_OBJECT_NAME, COALESCE(COALESCE(AUDITDATA.REMOTE_AUDIT_TRAIL.OBJ_PRIVILEGE, AUDITDATA.REMOTE_AUDIT_TRAIL.SYS_PRIVILEGE), 'N/A') AS RAT_PRIVILEGE_NAME, COALESCE(AUDITDATA.REMOTE_AUDIT_TRAIL.GRANTEE, 'N/A') AS RAT_GRANTEE, COALESCE(AUDITDATA.REMOTE_AUDIT_TRAIL.AUDIT_OPTION, 'N/A') AS RAT_AUDIT_OPTION, AUDITDATA.REMOTE_AUDIT_TRAIL.SESSIONID, COALESCE(TO_CHAR(STAFF_USER_ACCOUNTS_ADMIN.STAFF_ID), 'N/A') AS NOMIS_STAFF_ID, COALESCE(STAFF_USER_ACCOUNTS_ADMIN.STAFF_USER_TYPE, 'N/A') AS NOMIS_USER_ACCOUNT_TYPE FROM AUDITDATA.REMOTE_AUDIT_TRAIL, AUDITREF.STAFF_USER_ACCOUNTS STAFF_USER_ACCOUNTS_ADMIN WHERE (AUDITDATA.REMOTE_AUDIT_TRAIL.USERNAME = STAFF_USER_ACCOUNTS_ADMIN.USERNAME (+)) AND ((NOT AUDITDATA.REMOTE_AUDIT_TRAIL.ACTION BETWEEN 100 AND 102) AND AUDITDATA.REMOTE_AUDIT_TRAIL.REMOTE_DB = 'CNOMIS' AND (TRUNC(AUDITDATA.REMOTE_AUDIT_TRAIL.TIMESTAMP) >= TRUNC(CURRENT_TIMESTAMP - 17)))), dataset_ AS (SELECT RAT_USERNAME, DATE_OF_ACTION_, TIME_OF_ACTION_, RAT_OS_LOGIN_NAME, RAT_CLIENT_HOST_NAME, RAT_TERMINAL_IDENTIFIER, RAT_ACTION_TYPE, RETURNCODE, RAT_OBJECT_OWNER, RAT_OBJECT_NAME, RAT_NEW_OBJECT_OWNER, RAT_NEW_OBJECT_NAME, RAT_PRIVILEGE_NAME, RAT_GRANTEE, RAT_AUDIT_OPTION, SESSIONID, NOMIS_STAFF_ID, NOMIS_USER_ACCOUNT_TYPE FROM dataset_base_)",
