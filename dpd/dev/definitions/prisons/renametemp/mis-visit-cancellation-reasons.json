      "id": "3375956/DP2",
      "name": "Visits",
      "description": "MIS - Visits",
      "datasource": "bodmis",
      "query": "dataset_base_ AS (SELECT AT_OFFENDER_BOOKING.OFFENDER_ID_DISPLAY, BODIMIS.FACT_OFFENDER_VISIT.OFFENDER_VISIT_VISITOR_ID_NK, BODIMIS.FACT_OFFENDER_VISIT.OFFENDER_VISIT_ID, AT_VISIT_ESTABLISHMENT.ESTABLISHMENT_CODE_NK, COUNT(DISTINCT (BODIMIS.FACT_OFFENDER_VISIT.OFFENDER_VISIT_VISITOR_ID_NK)) AS COUNT_OF_VISIT_VISITOR_ID, COUNT(DISTINCT (BODIMIS.FACT_OFFENDER_VISIT.OFFENDER_VISIT_ID)) AS COUNT_OF_VISIT_ID, AT_VISIT_DATE.DAY_DATE, AT_VISIT_OUTCOME.DESCRIPTION, AT_VISIT_OFFENDER_CONTACTS.FIRST_NAME, AT_VISIT_OFFENDER_CONTACTS.LAST_NAME, AT_VISIT_ESTABLISHMENT.ESTABLISHMENT_DESCRIPTION, AT_VISIT_TYPE.DESCRIPTION AS VISIT_TYPE_DESCRIPTION FROM BODIMIS.DIM_DATE AT_VISIT_DATE, BODIMIS.DIM_REFERENCE_CODES AT_VISIT_OUTCOME, BODIMIS.DIM_REFERENCE_CODES AT_VISIT_TYPE, BODIMIS.DIM_REFERENCE_CODES AT_VISIT_STATUS, BODIMIS.DIM_ESTABLISHMENT AT_VISIT_ESTABLISHMENT, BODIMIS.DIM_OFFENDER_BOOKING AT_OFFENDER_BOOKING, BODIMIS.FACT_OFFENDER_VISIT, BODIMIS.DIM_OFFENDER_OTHER_CONTACTS AT_VISIT_OFFENDER_CONTACTS WHERE (AT_OFFENDER_BOOKING.OFFENDER_BOOKING_SK = BODIMIS.FACT_OFFENDER_VISIT.OFFENDER_BOOKING_SK) AND (BODIMIS.FACT_OFFENDER_VISIT.ESTABLISHMENT_SK = AT_VISIT_ESTABLISHMENT.ESTABLISHMENT_SK) AND (BODIMIS.FACT_OFFENDER_VISIT.VISIT_DATE_SK = AT_VISIT_DATE.DATE_SK) AND (BODIMIS.FACT_OFFENDER_VISIT.OTHER_CONTACT_SK = AT_VISIT_OFFENDER_CONTACTS.OTHER_CONTACT_SK (+)) AND (AT_VISIT_TYPE.REFERENCE_CODE_SK = BODIMIS.FACT_OFFENDER_VISIT.VISIT_TYPE_SK) AND (AT_VISIT_STATUS.REFERENCE_CODE_SK = BODIMIS.FACT_OFFENDER_VISIT.VISIT_STATUS_SK) AND (AT_VISIT_OUTCOME.REFERENCE_CODE_SK = BODIMIS.FACT_OFFENDER_VISIT.OUTCOME_REASON_SK) AND (AT_OFFENDER_BOOKING.OFFENDER_BOOKING_SK IN (SELECT CSL.OFFENDER_BOOKING_SK FROM BODIMIS.DIM_STAFF_ACCESSIBLE_CASELOAD SAC, BODIMIS.DIM_CASELOAD_SECURITY_LIST CSL WHERE SAC.CASELOAD_ID = CSL.CASELOAD_CODE AND SAC.USERNAME = (SELECT username FROM context_) AND CSL.ENDYEAR >= TO_CHAR(CURRENT_TIMESTAMP, 'YYYY') - 6)) AND (AT_VISIT_DATE.DAY_DATE BETWEEN TO_DATE('04-03-2024 00:00:00', 'DD-MM-YYYY HH24:MI:SS') AND TO_DATE('10-03-2024 00:00:00', 'DD-MM-YYYY HH24:MI:SS') AND AT_VISIT_STATUS.CODE IN ('CANC')) GROUP BY AT_OFFENDER_BOOKING.OFFENDER_ID_DISPLAY, BODIMIS.FACT_OFFENDER_VISIT.OFFENDER_VISIT_VISITOR_ID_NK, BODIMIS.FACT_OFFENDER_VISIT.OFFENDER_VISIT_ID, AT_VISIT_ESTABLISHMENT.ESTABLISHMENT_CODE_NK, AT_VISIT_DATE.DAY_DATE, AT_VISIT_OUTCOME.DESCRIPTION, AT_VISIT_OFFENDER_CONTACTS.FIRST_NAME, AT_VISIT_OFFENDER_CONTACTS.LAST_NAME, AT_VISIT_ESTABLISHMENT.ESTABLISHMENT_DESCRIPTION, AT_VISIT_TYPE.DESCRIPTION), dataset_ AS (SELECT OFFENDER_ID_DISPLAY, OFFENDER_VISIT_VISITOR_ID_NK, OFFENDER_VISIT_ID, ESTABLISHMENT_CODE_NK, COUNT_OF_VISIT_VISITOR_ID, COUNT_OF_VISIT_ID, DAY_DATE, DESCRIPTION, FIRST_NAME, LAST_NAME, ESTABLISHMENT_DESCRIPTION, VISIT_TYPE_DESCRIPTION, COUNT(OFFENDER_VISIT_ID) AS V_COUNT_VISIT_ID FROM dataset_base_)",
