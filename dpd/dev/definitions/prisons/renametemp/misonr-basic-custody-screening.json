      "datasource": "bodmis",
      "query": "dataset_base_ AS (SELECT ASSESSMENT_BCS_CASEID.RECEPTION_DATE, COUNT(DISTINCT ASSESSMENT.ASSESSMENT_ID) AS COUNT_OF_ASSESSMENTS, BCS_PART_STATUS.DESCRIPTION, ASSESSMENT_BCS_CASEID.BCS_PART1_COMP_DATE, ASSESSMENT_BCS_CASEID.BCS_PART1_EST_NAME, ASSESSMENT.ASSESSMENT_ID, ASSESSMENT_BCS_CASEID.BCS_PART1_EST_OPOL, OFFENDER.FAMILY_NAME, COALESCE(SUBSTR(OFFENDER.FORENAME, 1, INSTR(OFFENDER.FORENAME, ' ')), OFFENDER.FORENAME) AS FIRST_NAME, OFFENDER.DATE_OF_BIRTH, OFFENDER.OFFENDER_ID_DISPLAY, ASSESSMENT_BCS_CASEID.BCS_SCREENER FROM ASSESSMENT, ASSESSMENT_BCS_CASEID, BCS_PART_STATUS, OFFENDER WHERE (OFFENDER.OFFENDER_ID = ASSESSMENT.OFFENDER_ID (+)) AND (ASSESSMENT.ASSESSMENT_ID = ASSESSMENT_BCS_CASEID.ASSESSMENT_ID AND ASSESSMENT.ASSESSMENT_TYPE_ELM = 'TR_BCS') AND (BCS_PART_STATUS.BCS_PART_STATUS_CODE (+) = ASSESSMENT_BCS_CASEID.BCS_PART1_STATUS) AND (SUBSTR(OFFENDER.OFFENDER_ID_DISPLAY, 1, 10) = TAGMIS_CASELOAD_SECURITY('CQI37G', OFFENDER.OFFENDER_ID_DISPLAY)) AND (ASSESSMENT_BCS_CASEID.BCS_SYSTEM_CREATED_IND = 1 AND ASSESSMENT_BCS_CASEID.RECEPTION_DATE BETWEEN TO_DATE('01-11-2015 00:00:00', 'DD-MM-YYYY HH24:MI:SS') AND TO_DATE('01-12-2015 00:00:00', 'DD-MM-YYYY HH24:MI:SS') AND ASSESSMENT_BCS_CASEID.BCS_PART1_EST_NAME = 'Brinsford (HMP)' AND (ASSESSMENT_BCS_CASEID.LOCKED_INCOMP_REASON_DESC <> 'Released prior to completion' OR ASSESSMENT_BCS_CASEID.LOCKED_INCOMP_REASON_DESC IS NULL)) GROUP BY ASSESSMENT_BCS_CASEID.RECEPTION_DATE, BCS_PART_STATUS.DESCRIPTION, ASSESSMENT_BCS_CASEID.BCS_PART1_COMP_DATE, ASSESSMENT_BCS_CASEID.BCS_PART1_EST_NAME, ASSESSMENT.ASSESSMENT_ID, ASSESSMENT_BCS_CASEID.BCS_PART1_EST_OPOL, OFFENDER.FAMILY_NAME, COALESCE(SUBSTR(OFFENDER.FORENAME, 1, INSTR(OFFENDER.FORENAME, ' ')), OFFENDER.FORENAME), OFFENDER.DATE_OF_BIRTH, OFFENDER.OFFENDER_ID_DISPLAY, ASSESSMENT_BCS_CASEID.BCS_SCREENER), dataset_ AS (SELECT RECEPTION_DATE, COUNT_OF_ASSESSMENTS, DESCRIPTION, BCS_PART1_COMP_DATE, BCS_PART1_EST_NAME, ASSESSMENT_ID, BCS_PART1_EST_OPOL, FAMILY_NAME, FIRST_NAME, DATE_OF_BIRTH, OFFENDER_ID_DISPLAY, BCS_SCREENER, TO_DATE(RECEPTION_DATE, 'dd/MM/yyyy') AS V_RECEPTION_DATE_DISPLAY, SUM(CASE WHEN RECEPTION_DATE <= V_TARGET_DATE AND DESCRIPTION = 'Complete' THEN 1 ELSE 0 END) AS V_COMPLETED_ON_TIME, SUM(CASE WHEN RECEPTION_DATE > V_TARGET_DATE AND DESCRIPTION = 'Complete' OR DESCRIPTION = 'Locked Incomplete' THEN 1 ELSE 0 END) AS V_NOT_COMPLETED_ON_TIME, SUM(CASE WHEN DESCRIPTION = 'Unallocated' OR DESCRIPTION = 'WIP' THEN 1 ELSE 0 END) AS V_WORK_IN_PROGRESS, (RECEPTION_DATE + 3) AS V_TARGET_DATE, CASE WHEN RECEPTION_DATE <= V_TARGET_DATE AND DESCRIPTION = 'Complete' THEN 'Yes' ELSE 'No' END AS V_COMPLETED_ON_TIME_DISPLAY FROM dataset_base_)",
