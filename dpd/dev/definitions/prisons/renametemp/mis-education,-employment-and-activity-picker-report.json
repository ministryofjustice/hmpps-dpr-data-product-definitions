t_) AND (CASE WHEN TRUNC(AT_FACT_OFFENDER_ACTIVITY.OFFENDER_END_DATE) <> '31/12/9999' THEN TRUNC(AT_FACT_OFFENDER_ACTIVITY.OFFENDER_END_DATE) END BETWEEN TO_DATE('01-11-2022 00:00:00', 'DD-MM-YYYY HH24:MI:SS') AND TO_DATE('08-11-2022 00:00:00', 'DD-MM-YYYY HH24:MI:SS') OR CASE WHEN TRUNC(AT_FACT_OFFENDER_ACTIVITY.OFFENDER_END_DATE) <> '31/12/9999' THEN TRUNC(AT_FACT_OFFENDER_ACTIVITY.OFFENDER_END_DATE) END IS NULL))), dataset_ AS (SELECT OFFENDER_ID_DISPLAY, SURNAME, DESCRIPTION, ACTIVITY_DESCRIPTION, OFFENDER_START_DATE, OFFENDER_END_DATE, OFFENDER_ALLOCATION_START_DATE, OFFENDER_ALLOCATION_END_DATE, FIRST_NAME, UNIT_4_SHORT_DESCRIPTION, IEP_BAND FROM dataset_base_)",
