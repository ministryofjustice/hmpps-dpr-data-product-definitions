LOAD SAC, BODIMIS.DIM_CASELOAD_SECURITY_LIST CSL WHERE SAC.CASELOAD_ID = CSL.CASELOAD_CODE AND SAC.USERNAME = (SELECT username FROM context_) AND CSL.ENDYEAR >= TO_CHAR(CURRENT_TIMESTAMP, 'YYYY') - 6)) AND (AT_OFFENDER_BOOKING.LIVING_UNIT_SK = AT_DIM_LOCATION.UNIT_4_SK (+)) AND ((AT_OFFENDER_BOOKING.BOOKING_STATUS = 'O') AND (AT_OFFENDER_BOOKING.MIS_CURRENT_RECORD_IND = 'Y') AND AT_ESTABLISHMENT.ESTABLISHMENT_CODE_NK = (SELECT establishment_code FROM prompt_))), dataset_ AS (SELECT OFFENDER_ID_DISPLAY, SURNAME, FIRST_NAME, UNIT_4_SHORT_DESCRIPTION, DESCRIPTION, CODE, EDUCATION_LEVEL_DESC, DAY_DATE, EDUCATION_END_DATE, COMMENT_TEXT, SCHOOL_NAME, SPECIAL_EDUCATION_FLAG, EDUCATION_SCHEDULE_DESC, IEP_BAND, 'TBD' AS V_EDU_START_DATE_FIX, 'TBD' AS V_EDU_END_DATE_FIX_ FROM dataset_base_)",
