      "description": "C-NOMIS Audit",
      "datasource": "nomis",
      "query": "dataset_base_ AS (SELECT DISTINCT AUDITREF.STAFF_MEMBERS.LAST_NAME, AUDITREF.STAFF_MEMBERS.FIRST_NAME, AUDITDATA.REMOTE_USERS.USERNAME, COALESCE(AUDITREF.STAFF_USER_ACCOUNTS.STAFF_USER_TYPE, 'N/A') AS NOMIS_USER_ACCOUNT_TYPE, AUDITREF.STAFF_USER_ACCOUNTS.WORKING_CASELOAD_ID, AUDITDATA.REMOTE_USERS.ACCOUNT_STATUS, TRUNC(AUDITDATA.REMOTE_USERS.LOCK_DATE) AS RU_USER_LOCK_DATE_, TRUNC(AUDITDATA.REMOTE_USERS.EXPIRY_DATE) AS RU_USER_EXPIRY_DATE_, TRUNC(AUDITDATA.REMOTE_USERS.CREATED) AS RU_USER_CREATION_DATE_, MAX(AUDITDATA.REMOTE_AUDIT_TRAIL.TIMESTAMP) AS DATE_OF_LAST_ACTION_ FROM AUDITDATA.REMOTE_AUDIT_TRAIL, AUDITREF.STAFF_MEMBERS, AUDITREF.STAFF_USER_ACCOUNTS, AUDITDATA.REMOTE_USERS WHERE (AUDITREF.STAFF_USER_ACCOUNTS.STAFF_ID = AUDITREF.STAFF_MEMBERS.STAFF_ID) AND (AUDITDATA.REMOTE_AUDIT_TRAIL.USERNAME = AUDITREF.STAFF_USER_ACCOUNTS.USERNAME) AND (AUDITDATA.REMOTE_USERS.USERNAME = AUDITREF.STAFF_USER_ACCOUNTS.USERNAME (+)) AND (AUDITDATA.REMOTE_USERS.RECORD_EXISTS = 'Y') AND (AUDITREF.STAFF_USER_ACCOUNTS.WORKING_CASELOAD_ID = (SELECT establishment_code FROM prompt_) AND AUDITDATA.REMOTE_AUDIT_TRAIL.REMOTE_DB = 'CNOMIS' AND AUDITDATA.REMOTE_AUDIT_TRAIL.ACTION BETWEEN 100 AND 102 AND (AUDITDATA.REMOTE_USERS.REMOTE_DB = 'CNOMIS')) GROUP BY AUDITREF.STAFF_MEMBERS.LAST_NAME, AUDITREF.STAFF_MEMBERS.FIRST_NAME, AUDITDATA.REMOTE_USERS.USERNAME, COALESCE(AUDITREF.STAFF_USER_ACCOUNTS.STAFF_USER_TYPE, 'N/A'), AUDITREF.STAFF_USER_ACCOUNTS.WORKING_CASELOAD_ID, AUDITDATA.REMOTE_USERS.ACCOUNT_STATUS, TRUNC(AUDITDATA.REMOTE_USERS.LOCK_DATE), TRUNC(AUDITDATA.REMOTE_USERS.EXPIRY_DATE), TRUNC(AUDITDATA.REMOTE_USERS.CREATED)), dataset_ AS (SELECT LAST_NAME, FIRST_NAME, USERNAME, NOMIS_USER_ACCOUNT_TYPE, WORKING_CASELOAD_ID, ACCOUNT_STATUS, RU_USER_LOCK_DATE_, RU_USER_EXPIRY_DATE_, RU_USER_CREATION_DATE_, DATE_OF_LAST_ACTION_, (TO_DATE(DATE_OF_LAST_ACTION_, 'DD/MM/YYYY') - TO_DATE(TRUNC(SYSDATE), 'DD/MM/YYYY')) AS V_DAYSOLD FROM dataset_base_)",
