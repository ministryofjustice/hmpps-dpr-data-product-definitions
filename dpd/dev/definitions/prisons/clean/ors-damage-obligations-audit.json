      "id": "3376816/DP1",
      "name": "Damage Obligations Audit",
      "description": "Finance",
      "datasource": "nomis",
      "query": "dataset_base_ AS (SELECT AT_DAMOBL_CASELOAD.DESCRIPTION AS DESCRIPTION, AT_OFFENDER.OFFENDER_ID_DISPLAY AS OFFENDER_ID_DISPLAY, AT_OFFENDER.LAST_NAME AS LAST_NAME, AT_OFFENDER.FIRST_NAME AS FIRST_NAME, AT_OFF_DAMAGE_OBL.OFFENDER_ID AS OFFENDER_ID, AT_OFF_DAMAGE_OBL.OFFENDER_DMG_OBLIGATION_ID AS OFFENDER_DMG_OBLIGATION_ID, AT_OFF_DAMAGE_OBL.REFERENCE_NO AS REFERENCE_NO, AT_OFF_DAMAGE_OBL.START_DATE AS START_DATE, AT_OFF_DAMAGE_OBL.END_DATE AS END_DATE, AT_OFF_DAMAGE_OBL.OBLIGATION_STATUS AS OBLIGATION_STATUS, SUM(AT_OFF_DAMAGE_OBL.AMOUNT_TO_PAY) AS AMOUNT_TO_PAY_WITH_SUM, SUM(AT_OFF_DAMAGE_OBL.PAID_AMOUNT) AS PAID_AMOUNT_WITH_SUM FROM OMS_OWNER.OFFENDERS AT_OFFENDER, OMS_OWNER.OFFENDER_DAMAGE_OBLIGATIONS AT_OFF_DAMAGE_OBL, CASELOADS AT_DAMOBL_CASELOAD WHERE (AT_DAMOBL_CASELOAD.CASELOAD_ID = AT_OFF_DAMAGE_OBL.AGY_LOC_ID) AND (AT_OFF_DAMAGE_OBL.OFFENDER_ID = AT_OFFENDER.ROOT_OFFENDER_ID AND AT_OFFENDER.root_offender_id = AT_OFFENDER.offender_id) AND (AT_DAMOBL_CASELOAD.CASELOAD_TYPE = 'INST') AND (AT_DAMOBL_CASELOAD.ACTIVE_FLAG = 'Y') AND (AT_DAMOBL_CASELOAD.TRUST_ACCOUNTS_FLAG = 'Y') AND ((AT_DAMOBL_CASELOAD.caseload_id =(SELECT establishment_code from prompt_)) AND AT_OFF_DAMAGE_OBL.START_DATE >= TO_DATE('01-02-2014 00:00:00', 'DD-MM-YYYY HH24:MI:SS') AND AT_OFF_DAMAGE_OBL.START_DATE <= TO_DATE('14-02-2014 00:00:00', 'DD-MM-YYYY HH24:MI:SS')) GROUP BY AT_DAMOBL_CASELOAD.DESCRIPTION, AT_OFFENDER.OFFENDER_ID_DISPLAY, AT_OFFENDER.LAST_NAME, AT_OFFENDER.FIRST_NAME, AT_OFF_DAMAGE_OBL.OFFENDER_ID, AT_OFF_DAMAGE_OBL.OFFENDER_DMG_OBLIGATION_ID, AT_OFF_DAMAGE_OBL.REFERENCE_NO, AT_OFF_DAMAGE_OBL.START_DATE, AT_OFF_DAMAGE_OBL.END_DATE, AT_OFF_DAMAGE_OBL.OBLIGATION_STATUS), dataset_ AS (SELECT DESCRIPTION, OFFENDER_ID_DISPLAY, LAST_NAME, FIRST_NAME, OFFENDER_ID, OFFENDER_DMG_OBLIGATION_ID, REFERENCE_NO, START_DATE, END_DATE, OBLIGATION_STATUS, AMOUNT_TO_PAY_WITH_SUM, PAID_AMOUNT_WITH_SUM FROM dataset_base_)",
