      "description": "Finance",
      "datasource": "nomis",
      "query": "dataset_base_ AS (SELECT AT_OFFENDER_TRANSACTIONS.AUDIT_USER_ID AS AUDIT_USER_ID, AT_OFFENDER_TRANSACTIONS.TXN_POSTING_TYPE AS TXN_POSTING_TYPE, AT_OFFENDER_TRANSACTIONS.TXN_ENTRY_SEQ AS TXN_ENTRY_SEQ, AT_OFFENDER_TRANSACTIONS.AUDIT_TIMESTAMP AS AUDIT_TIMESTAMP, AT_OFFENDER_TRANSACTIONS.TXN_ID AS TXN_ID, AT_TRANSACTION_TYPES.DESCRIPTION AS DESCRIPTION, CASE WHEN AT_OFFENDER_TRANSACTIONS.SUB_ACCOUNT_TYPE = 'SAV' THEN 'Savings' WHEN AT_OFFENDER_TRANSACTIONS.SUB_ACCOUNT_TYPE = 'SPND' THEN 'Spends' WHEN AT_OFFENDER_TRANSACTIONS.SUB_ACCOUNT_TYPE = 'REG' THEN 'Private Cash' END AS SUB_ACCOUNT_NAME, AT_OFFENDER_TRANSACTIONS.TXN_ENTRY_AMOUNT AS TXN_ENTRY_AMOUNT, AT_TXN_OFFENDER.OFFENDER_ID_DISPLAY AS OFFENDER_ID_DISPLAY, AT_TXN_OFFENDER.FIRST_NAME AS FIRST_NAME, AT_TXN_OFFENDER.LAST_NAME AS LAST_NAME FROM OMS_OWNER.OFFENDERS AT_TXN_OFFENDER, USER_ACCESSIBLE_CASELOADS AT_USER_TXN_CASELOADS, OMS_OWNER.OFFENDER_TRANSACTIONS AT_OFFENDER_TRANSACTIONS, OMS_OWNER.TRANSACTION_TYPES AT_TRANSACTION_TYPES WHERE (AT_USER_TXN_CASELOADS.USERNAME = UPPER(SELECT username from context_)) AND (AT_USER_TXN_CASELOADS.CASELOAD_ID = AT_OFFENDER_TRANSACTIONS.CASELOAD_ID) AND (AT_TRANSACTION_TYPES.TXN_TYPE = AT_OFFENDER_TRANSACTIONS.TXN_TYPE) AND (AT_TRANSACTION_TYPES.ACTIVE_FLAG = 'Y') AND (AT_OFFENDER_TRANSACTIONS.OFFENDER_ID = AT_TXN_OFFENDER.ROOT_OFFENDER_ID) AND (AT_TXN_OFFENDER.ALIAS_OFFENDER_ID IS NULL) AND (AT_USER_TXN_CASELOADS.CASELOAD_ID = 'CFI' AND TRUNC(AT_OFFENDER_TRANSACTIONS.TXN_ENTRY_DATE) = TO_DATE('26-05-2016 00:00:00', 'DD-MM-YYYY HH24:MI:SS'))), dataset_ AS (SELECT AUDIT_USER_ID, TXN_POSTING_TYPE, TXN_ENTRY_SEQ, AUDIT_TIMESTAMP, TXN_ID, DESCRIPTION, SUB_ACCOUNT_NAME, TXN_ENTRY_AMOUNT, OFFENDER_ID_DISPLAY, FIRST_NAME, LAST_NAME FROM dataset_base_)",
