      "id": "3377299/DP3",
      "name": "Internal Locations",
      "description": "Demographic",
      "datasource": "nomis",
      "query": "dataset_base_ AS (SELECT AT_AGENCY_INTERNAL_LOCATION.INTERNAL_LOCATION_CODE AS INTERNAL_LOCATION_CODE, AT_AGENCY_INTERNAL_LOCATION.DESCRIPTION AS DESCRIPTION, AT_AGENCY_INTERNAL_LOCATION.USER_DESC AS USER_DESC, AT_AGENCY_INTERNAL_LOCATION.UNIT_TYPE AS UNIT_TYPE, AT_AGENCY_INTERNAL_LOCATION.CAPACITY AS CAPACITY, AT_AGENCY_INTERNAL_LOCATION.LIST_SEQ AS LIST_SEQ, CASE WHEN AT_LU.INTERNAL_LOCATION_USAGE = 'OIC' THEN 'Adjudication Hearing Location' WHEN AT_LU.INTERNAL_LOCATION_USAGE = 'APP' THEN 'Appointment Location' WHEN AT_LU.INTERNAL_LOCATION_USAGE = 'PROG' THEN 'Programme & Activities Location' WHEN AT_LU.INTERNAL_LOCATION_USAGE = 'PROP' THEN 'Property Location' WHEN AT_LU.INTERNAL_LOCATION_USAGE = 'MOVEMENT' THEN 'Prisoner Movement Location' WHEN AT_LU.INTERNAL_LOCATION_USAGE = 'OCCUR' THEN 'Occurence Location' WHEN AT_LU.INTERNAL_LOCATION_USAGE = 'VISIT' THEN 'Visits Location' ELSE AT_LU.INTERNAL_LOCATION_USAGE END AS USAGE, AT_LUL.USAGE_LOCATION_TYPE AS USAGE_LOCATION_TYPE FROM OMS_OWNER.AGENCY_INTERNAL_LOCATIONS AT_AGENCY_INTERNAL_LOCATION, OMS_OWNER.AGENCY_LOCATIONS AT_INT_LOC_EST, OMS_OWNER.INTERNAL_LOCATION_USAGES AT_LU, OMS_OWNER.INT_LOC_USAGE_LOCATIONS AT_LUL WHERE (AT_LUL.INTERNAL_LOCATION_USAGE_ID = AT_LU.INTERNAL_LOCATION_USAGE_ID) AND (AT_LUL.INTERNAL_LOCATION_ID = AT_AGENCY_INTERNAL_LOCATION.INTERNAL_LOCATION_ID) AND (AT_AGENCY_INTERNAL_LOCATION.ACTIVE_FLAG = 'Y') AND (AT_INT_LOC_EST.AGY_LOC_ID = AT_LU.AGY_LOC_ID) AND (AT_INT_LOC_EST.ACTIVE_FLAG = 'Y') AND (AT_INT_LOC_EST.AGENCY_LOCATION_TYPE = 'INST') AND AT_INT_LOC_EST.AGY_LOC_ID =(SELECT establishment_code from prompt_)), dataset_ AS (SELECT INTERNAL_LOCATION_CODE, DESCRIPTION, USER_DESC, UNIT_TYPE, CAPACITY, LIST_SEQ, USAGE, USAGE_LOCATION_TYPE FROM dataset_base_)",
      "parameters": [
