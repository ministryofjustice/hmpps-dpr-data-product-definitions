{
  "id": "ok6ud04ABUhmCDgARYWD8K7BY_Y",
  "name": "ORS Property Boxes Allocated to Transferred or Inactive Prisoners",
  "description": "This report returns a list of property boxes and the names of the active in transfer or inactive prisoners to which they are still allocated. There are two tabs in the report; the first shows property allocations where a location has been entered and the second tab shows property which does not have a location. <br/>ACC0017 - v00.00.02 Last Modified: 07/08/12",
  "metadata": {
    "version": "1.0.0",
    "tags": [
      "ACC0017 v00.00.01 LSRIT039"
    ]
  },
  "datasource": [
    {
      "id": "nomis",
      "name": "NOMIS",
      "database": "DIGITAL_PRISON_REPORTING",
      "catalog": "nomis"
    }
  ],
  "dataset": [
    {
      "id": "3377299/DP10",
      "name": "Prop Box Recycling",
      "description": "Demographic",
      "datasource": "nomis",
      "query": "dataset_base_ AS (SELECT DT_PROPERTY_BOXES.SEAL_MARK AS SEAL_MARK, AT_PROP_LOC.DESCRIPTION AS DESCRIPTION, DT_PROPERTY_BOXES.PROPERTY_CONTAINER_ID AS PROPERTY_CONTAINER_ID, DT_PROPERTY_BOXES.ACTIVE_FLAG AS ACTIVE_FLAG, AT_OFFENDER.OFFENDER_ID_DISPLAY AS OFFENDER_ID_DISPLAY, AT_OFFENDER.LAST_NAME AS LAST_NAME, AT_HEADER_BLOCK.STATUS_DISPLAY || ' ' || AT_HEADER_BLOCK.IN_OUT_STATUS AS COMMUNITY_STATUS FROM OMS_OWNER.OFFENDERS AT_OFFENDER, OMS_OWNER.OFFENDER_BOOKINGS AT_OFFENDER_BOOKING, OMS_OWNER.V_HEADER_BLOCK AT_HEADER_BLOCK, OMS_OWNER.AGENCY_INTERNAL_LOCATIONS AT_PROP_LOC, (SELECT Offender_Book_ID, Property_Container_ID, Active_Flag, Seal_Mark, Agy_Loc_ID, Expiry_Date, Internal_Location_ID, PROPOSED_DISPOSAL_DATE, Container_Code, Property_Only_Flag, Comment_Text FROM oms_owner.offender_ppty_containers WHERE active_flag = 'Y' AND Agy_Loc_ID = (SELECT establishment_code from prompt_)) DT_PROPERTY_BOXES WHERE (AT_OFFENDER.OFFENDER_ID = AT_OFFENDER_BOOKING.OFFENDER_ID) AND (AT_OFFENDER_BOOKING.OFFENDER_BOOK_ID = DT_PROPERTY_BOXES.OFFENDER_BOOK_ID (+)) AND (AT_OFFENDER_BOOKING.OFFENDER_BOOK_ID = AT_HEADER_BLOCK.OFFENDER_BOOK_ID (+)) AND (DT_PROPERTY_BOXES.INTERNAL_LOCATION_ID = AT_PROP_LOC.INTERNAL_LOCATION_ID (+)) AND (AT_HEADER_BLOCK.STATUS_DISPLAY || ' ' || AT_HEADER_BLOCK.IN_OUT_STATUS IN ('Active TRN', 'Historic OUT', 'Inactive OUT', 'Inactive IN', 'Historic IN') AND DT_PROPERTY_BOXES.ACTIVE_FLAG = 'Y')), dataset_ AS (SELECT SEAL_MARK, DESCRIPTION, PROPERTY_CONTAINER_ID, ACTIVE_FLAG, OFFENDER_ID_DISPLAY, LAST_NAME, COMMUNITY_STATUS FROM dataset_base_)",
      "parameters": [
        {
          "index": 0,
          "name": "establishment_code",
          "filterType": "autocomplete",
          "referenceType": "establishment",
          "reportFieldType": "string",
          "display": "Establishment",
          "description": "@Prompt('Establishment Code','A','Establishment\\Establishment Code',mono,free,,Not_Persistent,,User:3)",
          "mandatory": "true"
        }
      ],
      "schema": {
