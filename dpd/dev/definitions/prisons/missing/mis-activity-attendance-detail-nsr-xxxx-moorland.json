      "id": "3376639/DP0",
      "name": "Activities",
      "description": "MIS - Demographics",
      "datasource": "bodmis",
      "query": "dataset_base_ AS (SELECT AT_ACTIVITY_ESTABLISHMENT.ESTABLISHMENT_CODE_NK, AT_ACTIVITY_ESTABLISHMENT.ESTABLISHMENT_DESCRIPTION, AT_DIM_PROGRAMME_SERVICE.DESCRIPTION, AT_DIM_PROGRAMME_TYPE.DESCRIPTION AS ACTIVITY_DESCRIPTION, TRUNC(AT_FACT_OFFENDER_ACTIVITY.START_TIME) AS ACTIVITY_SESSION_DATE, AT_FACT_OFFENDER_ACTIVITY.STATUS, AT_FACT_OFFENDER_ACTIVITY.START_TIME, AT_FACT_OFFENDER_ACTIVITY.END_TIME, ((AT_FACT_OFFENDER_ACTIVITY.END_TIME - AT_FACT_OFFENDER_ACTIVITY.START_TIME) * 24 * 60 * 60) / 60 / 60 AS ACTIVITY_SESSION_DURATION, COALESCE(AT_FACT_OFFENDER_ACTIVITY.SLOT_CATEGORY_CODE, 'No Slot') AS SLOT_AS_DIMENSION, AT_OFFENDER_BOOKING.OFFENDER_ID_DISPLAY, AT_FACT_OFFENDER_ACTIVITY.ALLOCATED_FLAG, AT_CELL.UNIT_1_CODE, AT_CELL.UNIT_4_SHORT_DESCRIPTION, AT_OFFENDER_BOOKING.SURNAME, AT_FACT_OFFENDER_ACTIVITY.COMMENT_TEXT FROM BODIMIS.FACT_OFFENDER_ACTIVITY AT_FACT_OFFENDER_ACTIVITY, BODIMIS.DIM_ESTABLISHMENT AT_ACTIVITY_ESTABLISHMENT, BODIMIS.DIM_PROGRAMME_SERVICE AT_DIM_PROGRAMME_SERVICE, BODIMIS.DIM_PROGRAMME_TYPE AT_DIM_PROGRAMME_TYPE, BODIMIS.DIM_OFFENDER_BOOKING AT_OFFENDER_BOOKING, BODIMIS.DIM_CELL AT_CELL WHERE (AT_CELL.UNIT_4_SK = AT_OFFENDER_BOOKING.LIVING_UNIT_SK) AND (AT_OFFENDER_BOOKING.OFFENDER_BOOKING_SK IN (SELECT CSL.OFFENDER_BOOKING_SK FROM BODIMIS.DIM_STAFF_ACCESSIBLE_CASELOAD SAC, BODIMIS.DIM_CASELOAD_SECURITY_LIST CSL WHERE SAC.CASELOAD_ID = CSL.CASELOAD_CODE AND SAC.USERNAME = UPPER((SELECT username FROM context_ /* = #Variable('BOUSER') */)) AND CSL.ENDYEAR >= TO_CHAR(CURRENT_TIMESTAMP, 'YYYY') - 6)) AND (AT_OFFENDER_BOOKING.OFFENDER_BOOKING_SK = AT_FACT_OFFENDER_ACTIVITY.OFFENDER_BOOKING_SK) AND (AT_DIM_PROGRAMME_TYPE.PROGRAMME_TYPE_SK = AT_FACT_OFFENDER_ACTIVITY.PROGRAMME_TYPE_SK) AND (AT_ACTIVITY_ESTABLISHMENT.ESTABLISHMENT_SK = AT_FACT_OFFENDER_ACTIVITY.ESTABLISHMENT_SK) AND (AT_DIM_PROGRAMME_SERVICE.PROGRAMME_SERVICE_SK = AT_FACT_OFFENDER_ACTIVITY.PROGRAMME_SERVICE_SK) AND (AT_ACTIVITY_ESTABLISHMENT.ESTABLISHMENT_CODE_NK = (SELECT ESTABLISHMENT_CODE FROM prompt_ /* = #prompt('Enter Establishment Code','A','Activities\\Activity Establishment Code',Mono,Free,Not_Persistent,,User:0) */) AND TRUNC(AT_FACT_OFFENDER_ACTIVITY.START_TIME) BETWEEN (SELECT START_DATE FROM prompt_ /* = #prompt('Enter Start Date','D',,Mono,Free,Not_Persistent,,User:1) */) AND (SELECT END_DATE FROM prompt_ /* = #prompt('Enter End Date','D',,Mono,Free,Not_Persistent,,User:2) */) AND AT_FACT_OFFENDER_ACTIVITY.ALLOCATED_FLAG = 'Y')), dataset_ AS (SELECT ESTABLISHMENT_CODE_NK, ESTABLISHMENT_DESCRIPTION, DESCRIPTION, ACTIVITY_DESCRIPTION, ACTIVITY_SESSION_DATE, STATUS, START_TIME, END_TIME, ACTIVITY_SESSION_DURATION, SLOT_AS_DIMENSION, OFFENDER_ID_DISPLAY, ALLOCATED_FLAG, UNIT_1_CODE, UNIT_4_SHORT_DESCRIPTION, SURNAME, COMMENT_TEXT, INITCAP(ACTIVITY_DESCRIPTION) AS VACTIVITYDESC, CASE WHEN STATUS = 'ACCAB' THEN 'Acceptable Absence' ELSE CASE WHEN STATUS = 'ATT' THEN 'Attended' ELSE CASE WHEN STATUS = 'UNBEH' THEN 'Attended Unacceptable Behaviour' ELSE CASE WHEN STATUS = 'CANC' THEN 'Cancelled' ELSE CASE WHEN STATUS = 'UNACAB' THEN 'Unacceptable Absence' ELSE CASE WHEN STATUS IS NULL THEN 'Not Recorded' ELSE CASE WHEN STATUS = 'NREQ' THEN 'Not Required' ELSE STATUS END END END END END END END AS VACTIVITYSTATUS FROM dataset_base_)",
