      "description": "ONR - Ad-hoc Reports",
      "datasource": "bodmis",
      "query": "dataset_base_ AS (SELECT ASSESSMENT_BCS_CASEID.RECEPTION_DATE AS RECEPTION_DATE, COUNT(DISTINCT ASSESSMENT.ASSESSMENT_ID) AS COUNT_OF_ASSESSMENTS, BCS_PART_STATUS.DESCRIPTION AS DESCRIPTION, ASSESSMENT_BCS_CASEID.BCS_PART1_COMP_DATE AS BCS_PART1_COMP_DATE, ASSESSMENT_BCS_CASEID.BCS_PART1_EST_NAME AS BCS_PART1_EST_NAME, ASSESSMENT.ASSESSMENT_ID AS ASSESSMENT_ID, ASSESSMENT_BCS_CASEID.BCS_PART1_EST_OPOL AS BCS_PART1_EST_OPOL, OFFENDER.FAMILY_NAME AS FAMILY_NAME, COALESCE(SUBSTR(OFFENDER.FORENAME, 1, INSTR(OFFENDER.FORENAME, ' ')), OFFENDER.FORENAME) AS FIRST_NAME, OFFENDER.DATE_OF_BIRTH AS DATE_OF_BIRTH, OFFENDER.OFFENDER_ID_DISPLAY AS OFFENDER_ID_DISPLAY, ASSESSMENT_BCS_CASEID.BCS_SCREENER AS BCS_SCREENER, ASSESSMENT_BCS_CASEID.DATE_COMPLETED AS DATE_COMPLETED, ASSESSMENT_BCS_CASEID.ACTUAL_RELEASE_DATE AS ACTUAL_RELEASE_DATE FROM ASSESSMENT, ASSESSMENT_BCS_CASEID, BCS_PART_STATUS, OFFENDER WHERE (OFFENDER.OFFENDER_ID = ASSESSMENT.OFFENDER_ID (+)) AND (ASSESSMENT.ASSESSMENT_ID = ASSESSMENT_BCS_CASEID.ASSESSMENT_ID AND ASSESSMENT.ASSESSMENT_TYPE_ELM = 'TR_BCS') AND (BCS_PART_STATUS.BCS_PART_STATUS_CODE (+) = ASSESSMENT_BCS_CASEID.BCS_PART1_STATUS) AND (SUBSTR(OFFENDER.OFFENDER_ID_DISPLAY, 1, 10) = TAGMIS_CASELOAD_SECURITY('CQI37G', OFFENDER.OFFENDER_ID_DISPLAY)) AND (ASSESSMENT_BCS_CASEID.BCS_SYSTEM_CREATED_IND = 1 AND ASSESSMENT_BCS_CASEID.RECEPTION_DATE BETWEEN TO_DATE('01-04-2015 00:00:00', 'DD-MM-YYYY HH24:MI:SS') AND TO_DATE('21-08-2015 00:00:00', 'DD-MM-YYYY HH24:MI:SS') AND ASSESSMENT_BCS_CASEID.BCS_PART1_EST_NAME = 'Leeds (HMP)') GROUP BY ASSESSMENT_BCS_CASEID.RECEPTION_DATE, BCS_PART_STATUS.DESCRIPTION, ASSESSMENT_BCS_CASEID.BCS_PART1_COMP_DATE, ASSESSMENT_BCS_CASEID.BCS_PART1_EST_NAME, ASSESSMENT.ASSESSMENT_ID, ASSESSMENT_BCS_CASEID.BCS_PART1_EST_OPOL, OFFENDER.FAMILY_NAME, COALESCE(SUBSTR(OFFENDER.FORENAME, 1, INSTR(OFFENDER.FORENAME, ' ')), OFFENDER.FORENAME), OFFENDER.DATE_OF_BIRTH, OFFENDER.OFFENDER_ID_DISPLAY, ASSESSMENT_BCS_CASEID.BCS_SCREENER, ASSESSMENT_BCS_CASEID.DATE_COMPLETED, ASSESSMENT_BCS_CASEID.ACTUAL_RELEASE_DATE), dataset_ AS (SELECT RECEPTION_DATE, COUNT_OF_ASSESSMENTS, DESCRIPTION, BCS_PART1_COMP_DATE, BCS_PART1_EST_NAME, ASSESSMENT_ID, BCS_PART1_EST_OPOL, FAMILY_NAME, FIRST_NAME, DATE_OF_BIRTH, OFFENDER_ID_DISPLAY, BCS_SCREENER, DATE_COMPLETED, ACTUAL_RELEASE_DATE FROM dataset_base_)",
