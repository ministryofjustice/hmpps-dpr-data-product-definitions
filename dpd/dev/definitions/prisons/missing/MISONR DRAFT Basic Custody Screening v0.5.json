      "id": "48.RS",
      "name": "BCS Summary",
      "classification": "OFFICIAL",
      "version": "1.0.0",
      "render": "HTML",
      "dataset": "3375896/DP0",
      "filter": {
        "name": "prefilter_",
        "query": "prefilter_ AS (SELECT * FROM dataset_ WHERE V_AGE_AT_RECEPTION >= 18)"
      },
      "specification": {
        "template": "list",
        "field": [
          {
            "name": "$ref:v Reception Date Display",
            "display": "",
            "formula": "=ToDate([Reception Date];\"dd/MM/yyyy\")",
            "visible": "true",
            "sortable": true,
            "defaultsort": false
          },
          {
            "name": "$ref:COUNT_OF_ASSESSMENTS",
            "display": "Count of Assessments",
            "formula": "",
            "visible": "true",
            "sortable": true,
            "defaultsort": false
          },
          {
            "name": "$ref:v Completed on Time",
            "display": "",
            "formula": "=Sum(If([BCS Part1 Completion Date]<=[v Target Date] And [BCS Part1 Status] = \"Complete\";1;0))",
            "visible": "true",
            "sortable": true,
            "defaultsort": false
          },
          {
            "name": "$ref:v Not Completed on Time",
            "display": "",
            "formula": "=Sum(If(\n([BCS Part1 Completion Date]>[v Target Date] And [BCS Part1 Status] = \"Complete\") \nOr \n([BCS Part1 Status] = \"Locked Incomplete\" And [v Locked Incomplete Before Target] = 0);1;0))",
            "visible": "true",
            "sortable": true,
            "defaultsort": false
          },
          {
            "name": "$ref:v Released Sum",
            "display": "",
            "formula": "=Sum([v Locked Incomplete Before Target])",
            "visible": "true",
            "sortable": true,
            "defaultsort": false
          },
          {
            "name": "$ref:v Work in Progress",
            "display": "",
            "formula": "=Sum(If([BCS Part1 Status] = \"Unallocated\" Or [BCS Part1 Status] = \"WIP\";1;0))",
            "visible": "true",
            "sortable": true,
            "defaultsort": false
          },
          {
            "name": "$ref:v % Completed on Time",
            "display": "",
            "formula": "=[v Completed on Time]/[Count of Assessments] Where ([v Locked Incomplete Before Target] = 0)",
            "visible": "true",
            "sortable": true,
            "defaultsort": false
          }
        ]
      }
    },
