      "name": "Query 1",
      "description": "MIS Demographics",
      "datasource": "bodmis",
      "query": "dataset_base_ AS (SELECT AT_OFFENDER_BOOKING.SURNAME, AT_OFFENDER_BOOKING.FIRST_NAME, AT_OFFENDER_BOOKING.OFFENDER_ID_DISPLAY, AT_OFFENDER_BOOKING.MIDDLE_NAME_1, CASE WHEN AT_DOB.DAY_DATE <> '31/12/9000' THEN AT_DOB.DAY_DATE END AS DATE_OF_BIRTH, AT_OFFENDER_ALIAS.SURNAME AS ALIAS_SURNAME, AT_OFFENDER_ALIAS.FIRST_NAME AS ALIAS_GIVEN_NAME_1, CASE WHEN AT_OFFENDER_BOOKING.REVIEW_SUP_LEVEL_TYPE = 'Pending' OR AT_OFFENDER_BOOKING.REVIEW_SUP_LEVEL_TYPE IS NULL THEN 'Unclassified' ELSE AT_OFFENDER_BOOKING.REVIEW_SUP_LEVEL_TYPE END AS CURRENT_SECURITY_CATEGORY, COALESCE(AT_OFFENDER_BOOKING.IEP_BAND, '** Incentive Level Missing **') AS IEP_BAND, AT_OFFENDER_BOOKING.NATIONALITY_DESCRIPTION, COALESCE(AT_OFFENDER_BOOKING.RELIGION_DESCRIPTION, 'Not Specified') AS RELIGION_DESCRIPTION, AT_DIM_PROGRAMME_TYPE.DESCRIPTION, AT_OFF_HOME_ADDRESS.STREET, AT_OFF_HOME_ADDRESS.LOCALITY, AT_OFF_HOME_ADDRESS.CITY, AT_OFF_HOME_ADDRESS.County, AT_OFF_HOME_ADDRESS.POST_CODE, TRUNC(CURRENT_TIMESTAMP) AS TODAY_, AT_CELL.UNIT_3_DESCRIPTION FROM BODIMIS.DIM_ESTABLISHMENT AT_ESTABLISHMENT, BODIMIS.DIM_OFFENDER_BOOKING AT_OFFENDER_BOOKING, BODIMIS.DIM_DATE AT_DOB, BODIMIS.DIM_OFFENDER_ALIAS AT_OFFENDER_ALIAS, BODIMIS.DIM_PROGRAMME_TYPE AT_DIM_PROGRAMME_TYPE, BODIMIS.DIM_OFFENDER_ADDRESS AT_OFF_HOME_ADDRESS, BODIMIS.DIM_CELL AT_CELL, BODIMIS.FACT_OFFENDER_ACTIVITY AT_FACT_OFFENDER_ACTIVITY WHERE (AT_OFFENDER_BOOKING.BIRTH_DATE_SK = AT_DOB.DATE_SK) AND (AT_OFFENDER_BOOKING.OFFENDER_BOOKING_SK = AT_OFFENDER_ALIAS.OFFENDER_BOOKING_SK (+)) AND (AT_CELL.ESTABLISHMENT_SK = AT_ESTABLISHMENT.ESTABLISHMENT_SK) AND (AT_CELL.UNIT_4_SK = AT_OFFENDER_BOOKING.LIVING_UNIT_SK) AND (AT_OFFENDER_BOOKING.OFFENDER_BOOKING_SK = AT_OFF_HOME_ADDRESS.OFFENDER_BOOKING_SK (+)) AND (AT_OFF_HOME_ADDRESS.HOME_USAGE (+) = 'Y') AND (AT_ESTABLISHMENT.ACTIVE_FLAG = 'Y') AND (AT_OFFENDER_BOOKING.OFFENDER_BOOKING_SK IN (SELECT CSL.OFFENDER_BOOKING_SK FROM BODIMIS.DIM_STAFF_ACCESSIBLE_CASELOAD SAC, BODIMIS.DIM_CASELOAD_SECURITY_LIST CSL WHERE SAC.CASELOAD_ID = CSL.CASELOAD_CODE AND SAC.USERNAME = (SELECT username FROM context_) AND (CSL.ENDYEAR >= TO_CHAR(CURRENT_TIMESTAMP, 'YYYY') - 6 OR CSL.ENDYEAR = '2999'))) AND (AT_OFF_HOME_ADDRESS.OFFENDER_BOOKING_SK IS NULL OR AT_OFF_HOME_ADDRESS.OFFENDER_BOOKING_SK IN (SELECT CSL.OFFENDER_BOOKING_SK FROM BODIMIS.DIM_STAFF_ACCESSIBLE_CASELOAD SAC, BODIMIS.DIM_CASELOAD_SECURITY_LIST CSL WHERE SAC.CASELOAD_ID = CSL.CASELOAD_CODE AND SAC.USERNAME = (SELECT username FROM context_) AND (CSL.ENDYEAR >= TO_CHAR(CURRENT_TIMESTAMP, 'YYYY') - 6 OR CSL.ENDYEAR = '2999'))) AND (AT_OFFENDER_ALIAS.OFFENDER_BOOKING_SK IS NULL OR AT_OFFENDER_ALIAS.OFFENDER_BOOKING_SK IN (SELECT CSL.OFFENDER_BOOKING_SK FROM BODIMIS.DIM_STAFF_ACCESSIBLE_CASELOAD SAC, BODIMIS.DIM_CASELOAD_SECURITY_LIST CSL WHERE SAC.CASELOAD_ID = CSL.CASELOAD_CODE AND SAC.USERNAME = (SELECT username FROM context_) AND (CSL.ENDYEAR >= TO_CHAR(CURRENT_TIMESTAMP, 'YYYY') - 6 OR CSL.ENDYEAR = '2999'))) AND (AT_OFFENDER_BOOKING.OFFENDER_BOOKING_SK = AT_FACT_OFFENDER_ACTIVITY.OFFENDER_BOOKING_SK (+)) AND (AT_DIM_PROGRAMME_TYPE.PROGRAMME_TYPE_SK (+) = AT_FACT_OFFENDER_ACTIVITY.PROGRAMME_TYPE_SK) AND ((AT_ESTABLISHMENT.ESTABLISHMENT_CODE_NK = (SELECT establishment_code FROM prompt_)) AND (AT_OFFENDER_BOOKING.ACTIVE_FLAG = 'Y'))), dataset_ AS (SELECT SURNAME, FIRST_NAME, OFFENDER_ID_DISPLAY, MIDDLE_NAME_1, DATE_OF_BIRTH, ALIAS_SURNAME, ALIAS_GIVEN_NAME_1, CURRENT_SECURITY_CATEGORY, IEP_BAND, NATIONALITY_DESCRIPTION, RELIGION_DESCRIPTION, DESCRIPTION, STREET, LOCALITY, CITY, COUNTY, POST_CODE, TODAY_, UNIT_3_DESCRIPTION FROM dataset_base_)",
