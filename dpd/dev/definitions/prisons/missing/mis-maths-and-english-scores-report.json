      "query": "dataset_base_ AS (SELECT DISTINCT AT_ESTABLISHMENT.ESTABLISHMENT_DESCRIPTION, AT_OFFENDER_BOOKING.OFFENDER_ID_DISPLAY, AT_OFFENDER_BOOKING.SURNAME, AT_OFFENDER_BOOKING.INITIALS, BODIMIS.DIM_OFFENDER_EDUCATION.EDUCATION_TYPE, BODIMIS.DIM_OFFENDER_EDUCATION.EDUCATION_SEQ, AT_EDUCATION_STUDY_AREA.CODE, AT_EDUCATION_STUDY_AREA.DESCRIPTION, AT_EDUCATION_LEVEL.CODE AS EDUCATION_LEVEL_CODE, AT_EDUCATION_LEVEL.DESCRIPTION AS EDUCATION_LEVEL_DESC, BODIMIS.DIM_OFFENDER_EDUCATION.NUMBER_OF_YEARS, BODIMIS.DIM_OFFENDER_EDUCATION.GRADUATION_YEAR, AT_EDUCATION_START_DATE.DAY_DATE, AT_EDUCATION_END_DATE.DAY_DATE AS EDUCATION_END_DATE, BODIMIS.DIM_OFFENDER_EDUCATION.COMMENT_TEXT, BODIMIS.DIM_OFFENDER_EDUCATION.SCHOOL_NAME, BODIMIS.DIM_OFFENDER_EDUCATION.SPECIAL_EDUCATION_FLAG, BODIMIS.DIM_OFFENDER_EDUCATION.STUDENT_ID, BODIMIS.DIM_OFFENDER_EDUCATION.EDUCATION_SCHEDULE, AT_EDUCATION_SCHEDULE.DESCRIPTION AS EDUCATION_SCHEDULE_DESC FROM BODIMIS.DIM_DATE AT_EDUCATION_START_DATE, BODIMIS.DIM_DATE AT_EDUCATION_END_DATE, BODIMIS.DIM_ESTABLISHMENT AT_ESTABLISHMENT, BODIMIS.DIM_OFFENDER_BOOKING AT_OFFENDER_BOOKING, BODIMIS.DIM_OFFENDER_EDUCATION, BODIMIS.DIM_REFERENCE_CODES AT_EDUCATION_STUDY_AREA, BODIMIS.DIM_REFERENCE_CODES AT_EDUCATION_LEVEL, BODIMIS.DIM_REFERENCE_CODES AT_EDUCATION_SCHEDULE, BODIMIS.DIM_CELL AT_CELL WHERE (AT_OFFENDER_BOOKING.OFFENDER_BOOKING_SK = BODIMIS.DIM_OFFENDER_EDUCATION.OFFENDER_BOOKING_SK (+)) AND (BODIMIS.DIM_OFFENDER_EDUCATION.START_DATE_SK = AT_EDUCATION_START_DATE.DATE_SK (+)) AND (BODIMIS.DIM_OFFENDER_EDUCATION.END_DATE_SK = AT_EDUCATION_END_DATE.DATE_SK (+)) AND (BODIMIS.DIM_OFFENDER_EDUCATION.STUDY_AREA_SK = AT_EDUCATION_STUDY_AREA.REFERENCE_CODE_SK) AND (BODIMIS.DIM_OFFENDER_EDUCATION.EDUCATION_LEVEL_SK = AT_EDUCATION_LEVEL.REFERENCE_CODE_SK) AND (BODIMIS.DIM_OFFENDER_EDUCATION.EDUCATION_SCHEDULE = AT_EDUCATION_SCHEDULE.CODE) AND (AT_EDUCATION_SCHEDULE.CODE_DOMAIN = 'EDU_SCHED') AND (AT_CELL.ESTABLISHMENT_SK = AT_ESTABLISHMENT.ESTABLISHMENT_SK) AND (AT_CELL.UNIT_4_SK = AT_OFFENDER_BOOKING.LIVING_UNIT_SK) AND (AT_ESTABLISHMENT.ACTIVE_FLAG = 'Y') AND (AT_OFFENDER_BOOKING.OFFENDER_BOOKING_SK IN (SELECT CSL.OFFENDER_BOOKING_SK FROM BODIMIS.DIM_STAFF_ACCESSIBLE_CASELOAD SAC, BODIMIS.DIM_CASELOAD_SECURITY_LIST CSL WHERE SAC.CASELOAD_ID = CSL.CASELOAD_CODE AND SAC.USERNAME = (SELECT username FROM context_) AND CSL.ENDYEAR >= TO_CHAR(CURRENT_TIMESTAMP, 'YYYY') - 6)) AND ((AT_ESTABLISHMENT.ESTABLISHMENT_CODE_NK IN ((SELECT establishment_code FROM prompt_)) OR 'All' IN ((SELECT establishment_code FROM prompt_))) AND (AT_OFFENDER_BOOKING.MIS_CURRENT_RECORD_IND = 'Y') AND (AT_OFFENDER_BOOKING.ACTIVE_FLAG = 'Y'))), dataset_ AS (SELECT ESTABLISHMENT_DESCRIPTION, OFFENDER_ID_DISPLAY, SURNAME, INITIALS, EDUCATION_TYPE, EDUCATION_SEQ, CODE, DESCRIPTION, EDUCATION_LEVEL_CODE, EDUCATION_LEVEL_DESC, NUMBER_OF_YEARS, GRADUATION_YEAR, DAY_DATE, EDUCATION_END_DATE, COMMENT_TEXT, SCHOOL_NAME, SPECIAL_EDUCATION_FLAG, STUDENT_ID, EDUCATION_SCHEDULE, EDUCATION_SCHEDULE_DESC FROM dataset_base_)",
