      "query": "dataset_base_ AS (SELECT AT_OFFENDER.OFFENDER_ID_DISPLAY, AT_OFFENDER_TRANSACTIONS.TXN_ENTRY_AMOUNT, AT_OFFENDER_TRANSACTIONS.TXN_ENTRY_DATE, AT_OFFENDER.FIRST_NAME, AT_OFFENDER.LAST_NAME, TRUNC(AT_OFFENDER_EXTERNAL_MOVEMENTS.MOVEMENT_DATE) AS MOVEMENT_DATE_, AT_MOV_TYPE_DESC.DESCRIPTION, AT_MOVEMENT_REASONS.DESCRIPTION AS MOVEMENT_REASON_DESCRIPTION, AT_MOV_LOC_FROM_OFFND_LOCTION.AGY_LOC_ID FROM OMS_OWNER.OFFENDERS AT_OFFENDER, OMS_OWNER.REFERENCE_CODES AT_MOV_TYPE_DESC, OMS_OWNER.OFFENDER_BOOKINGS AT_OFFENDER_BOOKING, USER_ACCESSIBLE_CASELOADS AT_USER_TXN_CASELOADS, OMS_OWNER.AGENCY_LOCATIONS AT_MOV_LOC_FROM_OFFND_LOCTION, OMS_OWNER.OFFENDER_EXTERNAL_MOVEMENTS AT_OFFENDER_EXTERNAL_MOVEMENTS, OMS_OWNER.OFFENDER_TRANSACTIONS AT_OFFENDER_TRANSACTIONS, OMS_OWNER.MOVEMENT_REASONS AT_MOVEMENT_REASONS, OMS_OWNER.TRANSACTION_TYPES AT_TRANSACTION_TYPES WHERE (AT_OFFENDER_TRANSACTIONS.OFFENDER_ID = AT_OFFENDER_BOOKING.ROOT_OFFENDER_ID) AND (AT_USER_TXN_CASELOADS.USERNAME = (SELECT username FROM context_)) AND (AT_USER_TXN_CASELOADS.CASELOAD_ID = AT_OFFENDER_TRANSACTIONS.CASELOAD_ID) AND (AT_TRANSACTION_TYPES.TXN_TYPE = AT_OFFENDER_TRANSACTIONS.TXN_TYPE) AND (AT_TRANSACTION_TYPES.ACTIVE_FLAG = 'Y') AND (AT_OFFENDER.OFFENDER_ID = AT_OFFENDER_BOOKING.OFFENDER_ID) AND (AT_OFFENDER_BOOKING.OFFENDER_BOOK_ID = AT_OFFENDER_EXTERNAL_MOVEMENTS.OFFENDER_BOOK_ID (+)) AND (AT_OFFENDER_EXTERNAL_MOVEMENTS.MOVEMENT_TYPE = AT_MOVEMENT_REASONS.MOVEMENT_TYPE AND AT_OFFENDER_EXTERNAL_MOVEMENTS.MOVEMENT_REASON_CODE = AT_MOVEMENT_REASONS.MOVEMENT_REASON_CODE) AND (AT_MOV_TYPE_DESC.CODE (+) = AT_OFFENDER_EXTERNAL_MOVEMENTS.MOVEMENT_TYPE) AND (AT_MOV_TYPE_DESC.DOMAIN (+) = ('MOVE_TYPE')) AND (AT_OFFENDER_EXTERNAL_MOVEMENTS.FROM_AGY_LOC_ID = AT_MOV_LOC_FROM_OFFND_LOCTION.AGY_LOC_ID (+)) AND (AT_MOV_LOC_FROM_OFFND_LOCTION.AGENCY_LOCATION_TYPE = 'INST') AND ((AT_OFFENDER_BOOKING.ACTIVE_FLAG = 'Y') AND AT_TRANSACTION_TYPES.TXN_TYPE IN ('TELE') AND AT_OFFENDER_TRANSACTIONS.TXN_ENTRY_DAT