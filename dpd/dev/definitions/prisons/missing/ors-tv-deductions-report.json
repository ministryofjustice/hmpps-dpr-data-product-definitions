OCATIONS.AGY_LOC_ID = (SELECT establishment_code FROM prompt_)) AND (AT_OFFENDER_BOOKING.ACTIVE_FLAG = 'Y'))), dataset_ AS (SELECT OFFENDER_ID_DISPLAY, FIRST_NAME, LAST_NAME, IEP_LEVEL, UNIT_DESCRIPTION_4_SHORT, CAPACITY, NO_OF_OCCUPANT, START_DATE AS V_SCHED_START_DATE, END_DATE AS V_SCHED_END_DATE, PAYMENT_AMOUNT AS V_SCHED_WEEKLY_AMOUNT, TRANSACTIONS.TXN_ENTRY_DATE AS V_TRANS_DATE, TRANSACTIONS.TXN_ENTRY_AMOUNT AS V_TRANS_AMOUNT FROM dataset_base_)",
