      "query": "dataset_base_ AS (SELECT DISTINCT AT_CASE_NOTE_AGENCY_LOC.AGY_LOC_ID, AT_RELIGION.DESCRIPTION, AT_OFFENDER.LAST_NAME, AT_OFFENDER_CASE_NOTES.CASE_NOTE_TEXT, AT_HEADER_BLOCK.STATUS_DISPLAY, AT_OFFENDER_BOOKING.IN_OUT_STATUS, AT_HEADER_BLOCK.STATUS_DISPLAY || ' ' || AT_HEADER_BLOCK.IN_OUT_STATUS AS COMMUNITY_STATUS, AT_OFFENDER_CASE_NOTES.CASE_NOTE_TYPE, AT_OFFENDER_CASE_NOTES.CASE_NOTE_ID, AT_OFFENDER_CASE_NOTES.CASE_NOTE_SUB_TYPE, AT_OFFENDER_CASE_NOTES.CONTACT_DATE, AT_OFFENDER.OFFENDER_ID_DISPLAY FROM OMS_OWNER.OFFENDERS AT_OFFENDER, OMS_OWNER.OFFENDER_BOOKINGS AT_OFFENDER_BOOKING, OMS_OWNER.AGENCY_LOCATIONS AT_CASE_NOTE_AGENCY_LOC, OMS_OWNER.OFFENDER_PROFILE_DETAILS AT_OFFENDER_RELIGION, OMS_OWNER.V_HEADER_BLOCK AT_HEADER_BLOCK, OMS_OWNER.PROFILE_CODES AT_RELIGION, OMS_OWNER.OFFENDER_CASE_NOTES AT_OFFENDER_CASE_NOTES WHERE (AT_OFFENDER.OFFENDER_ID = AT_OFFENDER_BOOKING.OFFENDER_ID) AND (AT_OFFENDER_RELIGION.OFFENDER_BOOK_ID (+) = AT_OFFENDER_BOOKING.OFFENDER_BOOK_ID) AND (AT_OFFENDER_RELIGION.PROFILE_TYPE (+) = 'RELF') AND (AT_RELIGION.PROFILE_CODE (+) = AT_OFFENDER_RELIGION.PROFILE_CODE) AND (AT_RELIGION.PROFILE_TYPE (+) = AT_OFFENDER_RELIGION.PROFILE_TYPE) AND (AT_OFFENDER_BOOKING.OFFENDER_BOOK_ID = AT_HEADER_BLOCK.OFFENDER_BOOK_ID (+)) AND (AT_OFFENDER_BOOKING.OFFENDER_BOOK_ID = AT_OFFENDER_CASE_NOTES.OFFENDER_BOOK_ID) AND (AT_OFFENDER_CASE_NOTES.AGY_LOC_ID = AT_CASE_NOTE_AGENCY_LOC.AGY_LOC_ID (+)) AND (AT_CASE_NOTE_AGENCY_LOC.AGENCY_LOCATION_TYPE (+) = 'INST') AND (AT_CASE_NOTE_AGENCY_LOC.DESCRIPTION IN ('BELMARSH (HMP)') AND AT_OFFENDER_CASE_NOTES.CONTACT_DATE BETWEEN TO_DATE('01-06-2023 00:00:00', 'DD-MM-YYYY HH24:MI:SS') AND TO_DATE('30-06-2023 00:00:00', 'DD-MM-YYYY HH24:MI:SS') AND AT_OFFENDER_CASE_NOTES.CASE_NOTE_TYPE IN ('CHAP'))), dataset_ AS (SELECT AGY_LOC_ID, DESCRIPTION, LAST_NAME, CASE_NOTE_TEXT, STATUS_DISPLAY, IN_OUT_STATUS, COMMUNITY_STATUS, CASE_NOTE_TYPE, CASE_NOTE_ID, CASE_NOTE_SUB_TYPE, CONTACT_DATE, OFFENDER_ID_DISPLAY, COUNT(CASE_NOTE_ID) AS V_COUNT_CASE_NOTES FROM dataset_base_)",
