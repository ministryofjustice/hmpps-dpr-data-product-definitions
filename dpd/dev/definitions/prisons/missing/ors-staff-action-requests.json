      "query": "dataset_base_ AS (SELECT AT_STAFF_ACTION_REQUESTS.ACTION_REQUEST_TYPE, AT_STAFF_ACTION_REQUESTS.STAFF_ACTION_REQUEST_ID, TO_NUMBER(TO_CHAR(TRUNC(AT_STAFF_ACTION_REQUESTS.CREATE_DATETIME), 'SYYYY')) AS YEAR_OF_CREATE_DATE, AT_STAFF_ACTION_REQUESTS.FIRST_NAME, AT_STAFF_ACTION_REQUESTS.MIDDLE_NAME, AT_STAFF_ACTION_REQUESTS.LAST_NAME, AT_STAFF_ACTION_REQUESTS.USERNAME, TRUNC(AT_STAFF_ACTION_REQUESTS.CREATE_DATETIME) AS CREATE_DATE, AT_STAFF_ACTION_REQUESTS.LOCAL_AUTHORITY_CODE, AT_STAFF_ACTION_REQUESTS.TO_LOCAL_AUTHORITY_CODE, AT_STAFF_ACTION_REQUESTS.STAFF_ID, AT_STAFF_ACTION_REQUESTS.CREATE_USER_ID, AT_STAFF_ACTION_REQUESTS.RESPONSE_DATETIME, AT_STAFF_ACTION_REQUESTS.APPLICATION_DATETIME, AT_STAFF_ACTION_REQUESTS.ACTION_REQUEST_SUB_STATUS, AT_STAFF_ACTION_REQUESTS.MODIFY_DATETIME FROM OMS_OWNER.STAFF_ACTION_REQUESTS AT_STAFF_ACTION_REQUESTS WHERE (TRUNC(AT_STAFF_ACTION_REQUESTS.CREATE_DATETIME) >= (SELECT UNKNOWN_PARAMETER FROM prompt_ /* = #prompt('Request Date Start','D','Requests\\Create Date',Mono,Free,Not_Persistent,,User:0) */) AND TRUNC(AT_STAFF_ACTION_REQUESTS.CREATE_DATETIME) <= (SELECT UNKNOWN_PARAMETER FROM prompt_ /* = #prompt('Request Date End','D','Requests\\Create Date',Mono,Free,Not_Persistent,,User:1) */))), dataset_ AS (SELECT ACTION_REQUEST_TYPE, STAFF_ACTION_REQUEST_ID, YEAR_OF_CREATE_DATE, FIRST_NAME, MIDDLE_NAME, LAST_NAME, USERNAME, CREATE_DATE, LOCAL_AUTHORITY_CODE, TO_LOCAL_AUTHORITY_CODE, STAFF_ID, CREATE_USER_ID, RESPONSE_DATETIME, APPLICATION_DATETIME, ACTION_REQUEST_SUB_STATUS, MODIFY_DATETIME, CASE WHEN ACTION_REQUEST_SUB_STATUS = 'COMPELTED' THEN 'COMPLETED' ELSE ACTION_REQUEST_SUB_STATUS END AS V_STATUS FROM dataset_base_)",
