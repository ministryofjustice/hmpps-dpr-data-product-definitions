    {
     "id": "dps-veterans-in-custody-establishment-summary",
      "name": "Establishment Summary of Veterans in Custody",
      "description": "Veterans",
      "datasource": "datamart",
      "query": "with dataset_base_ as (SELECT offr.offender_id_display AS PRISONER_NUMBER, offr.birth_date AS DOB, date_diff('year', offr.birth_date, current_date) - CASE WHEN TO_CHAR(current_date, 'MMDD') < TO_CHAR(birth_date, 'MMDD') THEN 1 ELSE 0 END AS AGE, off_gen.description AS GENDER, off_eth.description AS ETHNICITY, CASE WHEN off_eth.code IN ('W2','W1','W9','W8','W10','W3') THEN 'White' WHEN off_eth.code IN ('O2','A4','O1','M9','M2','B9','B1','A3','A1','O9','M3','M1','B2','A9','A2') THEN 'BAME' WHEN off_eth.code = 'NS' THEN 'Not Stated' END AS GROUPED_ETHNICITY, off_loc.description AS PRISON, CASE WHEN off_loc.agy_loc_id IN ('AGI','BZI','DHI','DWI','ESI','EWI','FHI','LNI','NHI','PFI','SDI','STI') THEN 'Female' ELSE off_reg.description END AS REGION, CASE WHEN off_mr.military_seq IS NOT NULL THEN 'Y' ELSE 'N' END AS HAS_MILITARY_RECORD, CASE WHEN off_al.alert_code IS NOT NULL THEN 'Y' ELSE 'N' END AS HAS_VETERAN_FLAG, off_mr.service_number AS SERVICE_NUMBER FROM datamart.prisons.nomis_offenders offr INNER JOIN datamart.prisons.nomis_offender_bookings off_bk ON offr.offender_id = off_bk.offender_id AND off_bk.active_flag = 'Y' INNER JOIN datamart.prisons.nomis_agency_locations off_loc ON off_bk.agy_loc_id = off_loc.agy_loc_id AND off_loc.agency_location_type = 'INST' LEFT JOIN datamart.prisons.nomis_offender_military_records off_mr ON off_bk.offender_book_id = off_mr.offender_book_id LEFT JOIN datamart.prisons.nomis_offender_alerts off_al ON off_al.offender_book_id = off_bk.offender_book_id AND off_al.alert_status = 'ACTIVE' AND off_al.alert_code = 'F1' LEFT JOIN datamart.prisons.nomis_reference_codes off_gen ON off_gen.code = offr.sex_code AND off_gen.domain = 'SEX' LEFT JOIN datamart.prisons.nomis_reference_codes off_eth ON offr.race_code = off_eth.code AND off_eth.domain = 'ETHNICITY' LEFT JOIN datamart.prisons.nomis_areas off_reg ON off_loc.noms_region_code = off_reg.area_code) SELECT PRISON, REGION, COUNT(*) AS total_population, COUNT(CASE WHEN HAS_MILITARY_RECORD = 'Y' OR HAS_VETERAN_FLAG = 'Y' THEN 1 END) AS total_veterans, COUNT(CASE WHEN HAS_MILITARY_RECORD = 'Y' THEN 1 END) AS total_veterans_with_record, COUNT(CASE WHEN HAS_VETERAN_FLAG = 'Y' THEN 1 END) AS total_veterans_with_alert, COUNT(CASE WHEN SERVICE_NUMBER is not null THEN 1 END) AS total_veterans_with_service_number, ROUND(COUNT(CASE WHEN HAS_MILITARY_RECORD = 'Y' OR HAS_VETERAN_FLAG = 'Y' THEN 1 END) * 100.0 / COUNT(*), 2) AS percentage_veterans, COUNT(CASE WHEN AGE < 25 THEN 1 END) AS population_under_25, COUNT(CASE WHEN (HAS_MILITARY_RECORD = 'Y' OR HAS_VETERAN_FLAG = 'Y') AND AGE < 25 THEN 1 END) AS veterans_under_25, ROUND(COUNT(CASE WHEN (HAS_MILITARY_RECORD = 'Y' OR HAS_VETERAN_FLAG = 'Y') AND AGE < 25 THEN 1 END) * 100.0 / COUNT(*), 2) AS percentage_veterans_under_25, COUNT(CASE WHEN AGE >= 25 AND AGE <= 29 THEN 1 END) AS population_25_to_29, COUNT(CASE WHEN (HAS_MILITARY_RECORD = 'Y' OR HAS_VETERAN_FLAG = 'Y') AND AGE >= 25 AND AGE <= 29 THEN 1 END) AS veterans_25_to_29, ROUND(COUNT(CASE WHEN (HAS_MILITARY_RECORD = 'Y' OR HAS_VETERAN_FLAG = 'Y') AND AGE >= 25 AND AGE <= 29 THEN 1 END) * 100.0 / COUNT(*), 2) AS percentage_veterans_25_to_29, COUNT(CASE WHEN AGE >= 30 AND AGE <= 39 THEN 1 END) AS population_30_to_39, COUNT(CASE WHEN (HAS_MILITARY_RECORD = 'Y' OR HAS_VETERAN_FLAG = 'Y') AND AGE >= 30 AND AGE <= 39 THEN 1 END) AS veterans_30_to_39, ROUND(COUNT(CASE WHEN (HAS_MILITARY_RECORD = 'Y' OR HAS_VETERAN_FLAG = 'Y') AND AGE >= 30 AND AGE <= 39 THEN 1 END) * 100.0 / COUNT(*), 2) AS percentage_veterans_30_to_39, COUNT(CASE WHEN AGE >= 40 AND AGE <= 49 THEN 1 END) AS population_40_to_49, COUNT(CASE WHEN (HAS_MILITARY_RECORD = 'Y' OR HAS_VETERAN_FLAG = 'Y') AND AGE >= 40 AND AGE <= 49 THEN 1 END) AS veterans_40_to_49, ROUND(COUNT(CASE WHEN (HAS_MILITARY_RECORD = 'Y' OR HAS_VETERAN_FLAG = 'Y') AND AGE >= 40 AND AGE <= 49 THEN 1 END) * 100.0 / COUNT(*), 2) AS percentage_veterans_40_to_49, COUNT(CASE WHEN AGE >= 50 THEN 1 END) AS population_over_50, COUNT(CASE WHEN (HAS_MILITARY_RECORD = 'Y' OR HAS_VETERAN_FLAG = 'Y') AND AGE >= 50 THEN 1 END) AS veterans_over_50, ROUND(COUNT(CASE WHEN (HAS_MILITARY_RECORD = 'Y' OR HAS_VETERAN_FLAG = 'Y') AND AGE >= 50 THEN 1 END) * 100.0 / COUNT(*), 2) AS percentage_veterans_over_50, COUNT(CASE WHEN GROUPED_ETHNICITY = 'BAME' THEN 1 END) AS population_bame, COUNT(CASE WHEN (HAS_MILITARY_RECORD = 'Y' OR HAS_VETERAN_FLAG = 'Y') AND GROUPED_ETHNICITY = 'BAME' THEN 1 END) AS veterans_bame, ROUND(COUNT(CASE WHEN (HAS_MILITARY_RECORD = 'Y' OR HAS_VETERAN_FLAG = 'Y') AND GROUPED_ETHNICITY = 'BAME' THEN 1 END) * 100.0 / COUNT(*), 2) AS percentage_veterans_bame, COUNT(CASE WHEN GROUPED_ETHNICITY = 'White' THEN 1 END) AS population_white, COUNT(CASE WHEN (HAS_MILITARY_RECORD = 'Y' OR HAS_VETERAN_FLAG = 'Y') AND GROUPED_ETHNICITY = 'White' THEN 1 END) AS veterans_white, ROUND(COUNT(CASE WHEN (HAS_MILITARY_RECORD = 'Y' OR HAS_VETERAN_FLAG = 'Y') AND GROUPED_ETHNICITY = 'White' THEN 1 END) * 100.0 / COUNT(*), 2) AS percentage_veterans_white, COUNT(CASE WHEN GROUPED_ETHNICITY = 'Not Stated' THEN 1 END) AS population_not_stated, COUNT(CASE WHEN (HAS_MILITARY_RECORD = 'Y' OR HAS_VETERAN_FLAG = 'Y') AND GROUPED_ETHNICITY = 'Not Stated' THEN 1 END) AS veterans_not_stated, ROUND(COUNT(CASE WHEN (HAS_MILITARY_RECORD = 'Y' OR HAS_VETERAN_FLAG = 'Y') AND GROUPED_ETHNICITY = 'Not Stated' THEN 1 END) * 100.0 / COUNT(*), 2) AS percentage_veterans_not_stated FROM dataset_base_ GROUP BY PRISON, REGION ORDER BY PRISON ASC",
      "parameters": [],
