  "dataset": [
    {
      "id": "dps-veterans-in-custody",
      "name": "Veterans in Custody",
      "description": "Veterans",
      "datasource": "datamart",
      "query": "SELECT offr.offender_id_display AS PRISONER_NUMBER, offr.first_name as FIRST_NAME, offr.last_name as LAST_NAME, offr.birth_date AS DOB, date_diff('year', offr.birth_date, current_date) - CASE WHEN TO_CHAR(current_date, 'MMDD') < TO_CHAR(birth_date, 'MMDD') THEN 1 ELSE 0 END AS AGE, off_gen.description AS GENDER, off_eth.description AS ETHNICITY, CASE WHEN off_eth.code IN ('W2','W1','W9','W8','W10','W3') THEN 'White' WHEN off_eth.code IN ('O2','A4','O1','M9','M2','B9','B1','A3','A1','O9','M3','M1','B2','A9','A2') THEN 'BAME' WHEN off_eth.code = 'NS' THEN 'Not Stated' END AS GROUPED_ETHNICITY, off_loc.description AS PRISON, CASE WHEN off_loc.agy_loc_id IN ('AGI','BZI','DHI','DWI','ESI','EWI','FHI','LNI','NHI','PFI','SDI','STI') THEN 'Female' ELSE off_reg.description END AS REGION, CASE WHEN off_mr.military_seq IS NOT NULL THEN 'Y' ELSE 'N' END AS HAS_MILITARY_RECORD, CASE WHEN off_al.alert_code IS NOT NULL THEN 'Y' ELSE 'N' END AS HAS_VETERAN_FLAG, off_mr.service_number AS SERVICE_NUMBER FROM datamart.prisons.nomis_offenders offr INNER JOIN datamart.prisons.nomis_offender_bookings off_bk ON offr.offender_id = off_bk.offender_id AND off_bk.active_flag = 'Y' INNER JOIN datamart.prisons.nomis_agency_locations off_loc ON off_bk.agy_loc_id = off_loc.agy_loc_id AND off_loc.agency_location_type = 'INST' LEFT JOIN datamart.prisons.nomis_offender_military_records off_mr ON off_bk.offender_book_id = off_mr.offender_book_id LEFT JOIN datamart.prisons.nomis_offender_alerts off_al ON off_al.offender_book_id = off_bk.offender_book_id AND off_al.alert_status = 'ACTIVE' AND off_al.alert_code = 'F1' LEFT JOIN datamart.prisons.nomis_reference_codes off_gen ON off_gen.code = offr.sex_code AND off_gen.domain = 'SEX' LEFT JOIN datamart.prisons.nomis_reference_codes off_eth ON offr.race_code = off_eth.code AND off_eth.domain = 'ETHNICITY' LEFT JOIN datamart.prisons.nomis_areas off_reg ON off_loc.noms_region_code = off_reg.area_code WHERE off_mr.military_seq IS NOT NULL OR off_al.alert_code IS NOT NULL",
