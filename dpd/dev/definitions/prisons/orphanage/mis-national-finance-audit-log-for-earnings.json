      "description": "MIS Finance",
      "datasource": "bodmis",
      "query": "dataset_base_ AS ( SELECT DISTINCT AT_CASELOADS.ESTABLISHMENT_CODE_NK, AT_BUSINESS_DATE.DAY_DATE AS BUSINESS_DATE, AT_DIM_DATE_TRANSACTIONS.DAY_DATE, AT_FACT_OFFENDER_TRANSACTION.TXN_ENTRY_TIME, COALESCE( AT_FACT_OFFENDER_TRANSACTION.MODIFY_USER_ID, COALESCE( AT_FACT_OFFENDER_TRANSACTION.CREATE_USER_ID, 'Not Recorded' ) ) AS TRANSACTION_CREATED_MODIFIED39, AT_DIM_TRANSACTION_TYPE.DESCRIPTION, AT_FACT_OFFENDER_TRANSACTION.TRANSACTION_ENTRY_AMOUNT, AT_DIM_TXN_POSTING_TYPE.TXN_POSTING_TYPE_DESCRIPTION, AT_FACT_OFFENDER_TRANSACTION.TXN_ID, AT_FACT_OFFENDER_TRANSACTION.TXN_ENTRY_SEQ, AT_DIM_ACCOUNT_CODE_TXN.ACCOUNT_NAME, AT_TXN_OB.SURNAME, AT_TXN_OB.INITIALS, AT_TXN_OB.OFFENDER_ID_DISPLAY FROM BODIMIS.FACT_OFFENDER_TRANSACTION AT_FACT_OFFENDER_TRANSACTION, BODIMIS.DIM_ESTABLISHMENT AT_CASELOADS, BODIMIS.DIM_DATE AT_DIM_DATE_TRANSACTIONS, BODIMIS.DIM_TRANSACTION_TYPE AT_DIM_TRANSACTION_TYPE, BODIMIS.DIM_TXN_POSTING_TYPE AT_DIM_TXN_POSTING_TYPE, BODIMIS.DIM_ACCOUNT_CODE AT_DIM_ACCOUNT_CODE_TXN, BODIMIS.DIM_OFFENDER_BOOKING AT_TXN_OB, BODIMIS.DIM_DATE AT_BUSINESS_DATE WHERE ( AT_DIM_TRANSACTION_TYPE.TRANSACTION_TYPE_SK = AT_FACT_OFFENDER_TRANSACTION.TRANSACTION_TYPE_SK ) AND ( AT_FACT_OFFENDER_TRANSACTION.TRANSACTION_POSTING_TYPE_SK = AT_DIM_TXN_POSTING_TYPE.TRANSACTION_POSTING_TYPE_SK ) AND ( AT_DIM_DATE_TRANSACTIONS.DATE_SK = AT_FACT_OFFENDER_TRANSACTION.TRANSACTION_ENTRY_DATE_SK ) AND ( AT_FACT_OFFENDER_TRANSACTION.ACCOUNT_CODE_SK = AT_DIM_ACCOUNT_CODE_TXN.ACCOUNT_CODE_SK ) AND ( AT_CASELOADS.ESTABLISHMENT_SK = AT_FACT_OFFENDER_TRANSACTION.ESTABLISHMENT_SK ) AND ( AT_BUSINESS_DATE.DATE_SK = AT_FACT_OFFENDER_TRANSACTION.TRANSACTION_BUSINESS_DATE_SK ) AND ( AT_TXN_OB.ALIAS_OFFENDER_SK = AT_FACT_OFFENDER_TRANSACTION.ALIAS_OFFENDER_SK AND AT_TXN_OB.BOOKING_SEQ = 1 ) AND ( AT_CASELOADS.ESTABLISHMENT_CODE_NK IN ( SELECT establishment_code FROM prompt_ ) AND AT_DIM_TRANSACTION_TYPE.TRANSACTION_TYPE IN ( SELECT transaction_type FROM prompt_ ) AND ( AT_DIM_DATE_TRANSACTIONS.DAY_DATE BETWEEN ( SELECT transaction_start_date FROM prompt_ ) AND ( SELECT transaction_end_date FROM prompt_ ) ) ) ), dataset_ AS ( SELECT DISTINCT ESTABLISHMENT_CODE_NK, BUSINESS_DATE, DAY_DATE, TXN_ENTRY_TIME, TRANSACTION_CREATED_MODIFIED39, DESCRIPTION, TRANSACTION_ENTRY_AMOUNT, TXN_POSTING_TYPE_DESCRIPTION, TXN_ID, TXN_ENTRY_SEQ, ACCOUNT_NAME, SURNAME, INITIALS, OFFENDER_ID_DISPLAY FROM dataset_base_ )",
      "parameters": [
