{
  "id": "ok9GYvIABHd_JqMAO9CD8K7BsgM",
  "name": "ORS Temporary Absence and Home Leave Return Dates",
  "description": "This report returns prisoners who are due to return to an establishment from a temporary absence or home leave movement between the dates entered at runtime. The prisoners shown on the report are those who have already left the establishment, having had the outward movement confirmed by Reception.<br/>MOV0029 - v00.00.04 Last Modified: 19/11/12",
  "metadata": {
    "version": "1.0.0",
    "tags": [
      "MOV0029 v00.00.04 DPR LSRIT039.1"
    ]
  },
  "datasource": [
    {
      "id": "nomis",
      "name": "NOMIS",
      "database": "DIGITAL_PRISON_REPORTING",
      "catalog": "nomis"
    }
  ],
  "dataset": [
    {
      "id": "3377299/DP0",
      "name": "Return Dates",
      "description": "OR - Demographics",
      "datasource": "nomis",
      "query": "dataset_base_ AS (SELECT AT_OFFENDER.OFFENDER_ID_DISPLAY AS OFFENDER_ID_DISPLAY, AT_OFFENDER.FIRST_NAME AS FIRST_NAME, AT_OFFENDER.LAST_NAME AS LAST_NAME, AT_HEADER_BLOCK.LIV_UNIT_DESC AS LIV_UNIT_DESC, AT_HEADER_BLOCK.STATUS_DISPLAY || ' ' || AT_HEADER_BLOCK.IN_OUT_STATUS AS COMMUNITY_STATUS, AT_TAP_RETURN.EVENT_DATE AS EVENT_DATE, AT_TAP_RETURN.START_TIME AS START_TIME, AT_TAP_RETURN.EVENT_SUB_TYPE_DESC AS EVENT_SUB_TYPE_DESC FROM OMS_OWNER.OFFENDERS AT_OFFENDER, OMS_OWNER.OFFENDER_IND_SCHEDULES AT_OFFENDER_IND_SCHEDULES, OMS_OWNER.OFFENDER_BOOKINGS AT_OFFENDER_BOOKING, USER_ACCESSIBLE_CASELOADS AT_UAC_ROTL, OMS_OWNER.AGENCY_LOCATIONS AT_RETURN_EST, OMS_OWNER.V_HEADER_BLOCK AT_HEADER_BLOCK, OMS_OWNER.V_OFFENDER_EXT_MOVEMENTS AT_TAP_RETURN WHERE (AT_OFFENDER.OFFENDER_ID = AT_OFFENDER_BOOKING.OFFENDER_ID) AND (AT_OFFENDER_BOOKING.OFFENDER_BOOK_ID = AT_OFFENDER_IND_SCHEDULES.OFFENDER_BOOK_ID) AND (AT_OFFENDER_BOOKING.OFFENDER_BOOK_ID = AT_HEADER_BLOCK.OFFENDER_BOOK_ID (+)) AND (AT_TAP_RETURN.EVENT_ID = AT_OFFENDER_IND_SCHEDULES.EVENT_ID) AND (AT_TAP_RETURN.OFFENDER_BOOK_ID = AT_OFFENDER_IND_SCHEDULES.OFFENDER_BOOK_ID) AND (AT_TAP_RETURN.EVENT_TYPE = 'TAP') AND (AT_TAP_RETURN.DIRECTION_CODE = 'IN') AND (AT_RETURN_EST.AGY_LOC_ID = AT_TAP_RETURN.TO_AGY_LOC_ID) AND (AT_RETURN_EST.AGENCY_LOCATION_TYPE = 'INST') AND (AT_RETURN_EST.AGY_LOC_ID = AT_UAC_ROTL.CASELOAD_ID) AND ((AT_OFFENDER_BOOKING.IN_OUT_STATUS = 'OUT') AND (AT_OFFENDER_BOOKING.ACTIVE_FLAG = 'Y') AND AT_TAP_RETURN.EVENT_DATE >= (SELECT START_DATE FROM prompt_ /* = #prompt('Enter Return Date (Start)','D',,Mono,Free,Not_Persistent,,User:0) */) AND AT_TAP_RETURN.EVENT_DATE <= (SELECT END_DATE FROM prompt_ /* = #prompt('Enter Return Date (End)','D',,Mono,Free,Not_Persistent,,User:1) */) AND AT_RETURN_EST.AGY_LOC_ID = (SELECT ESTABLISHMENT_CODE FROM prompt_ /* = #prompt('Establishment Code','A','TAP Return\\Returning To Establishment Code',Mono,Free,Not_Persistent,,User:2) */) AND (AT_UAC_ROTL.USERNAME = UPPER((SELECT username FROM context_ /* = #Variable('BOUSER') */))))), dataset_ AS (SELECT OFFENDER_ID_DISPLAY, FIRST_NAME, LAST_NAME, LIV_UNIT_DESC, COMMUNITY_STATUS, EVENT_DATE, START_TIME, EVENT_SUB_TYPE_DESC FROM dataset_base_)",
      "parameters": [
        {
          "index": 0,
          "name": "start_date",
          "filterType": "date",
          "reportFieldType": "date",
          "display": "Enter Return Date (Start)",
          "description": "@prompt('Enter Return Date (Start)','D',,Mono,Free,Not_Persistent,,User:0)",
          "mandatory": "true"
        },
        {
          "index": 1,
          "name": "end_date",
          "filterType": "date",
          "reportFieldType": "date",
          "display": "Enter Return Date (End)",
          "description": "@prompt('Enter Return Date (End)','D',,Mono,Free,Not_Persistent,,User:1)",
          "mandatory": "true"
        },
        {
          "index": 2,
          "name": "establishment_code",
          "filterType": "text",
          "reportFieldType": "string",
          "display": "Establishment Code",
          "description": "@prompt('Establishment Code','A','TAP Return\\Returning To Establishment Code',Mono,Free,Not_Persistent,,User:2)",
          "mandatory": "true"
        }
      ],
