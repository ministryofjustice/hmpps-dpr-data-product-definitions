    {
      "id": "3376286/DP3",
      "name": "Occurred Movements",
      "description": "OR - Sentence",
      "datasource": "nomis",
      "query": "dataset_base_ AS (SELECT DISTINCT AT_OFFENDER.OFFENDER_ID_DISPLAY AS OFFENDER_ID_DISPLAY, AT_OFFENDER.LAST_NAME AS LAST_NAME, AT_OFFENDER_PORT_REF.IDENTIFIER AS IDENTIFIER, AT_OFFENDER_CID.IDENTIFIER AS CID, AT_OFFENDER_HOREF.IDENTIFIER AS HOREF, AT_OFFENDER.FIRST_NAME AS FIRST_NAME, AT_OFFENDER.BIRTH_DATE AS BIRTH_DATE, AT_OFFENDER_EXTERNAL_MOVEMENTS.MOVEMENT_DATE AS MOVEMENT_DATE, AT_OFFENDER_EXTERNAL_MOVEMENTS.MOVEMENT_TIME AS MOVEMENT_TIME, AT_MOV_TYPE_DESC.CODE AS CODE, AT_MOVEMENT_REASONS.DESCRIPTION AS DESCRIPTION, AT_OFFENDER_EXTERNAL_MOVEMENTS.DIRECTION_CODE AS DIRECTION_CODE, COALESCE(AT_EXT_MOV_FROM_AGY.DESCRIPTION, AT_FROM_CITY_CODES.DESCRIPTION, AT_FROM_ADDRESS_ATTENDED.STREET || ' ' || AT_FROM_ADDRESS_ATTENDED.LOCALITY || ' ' || AT_FROM_ADDRESS_ATTENDED.POSTAL_CODE) AS FROM_, COALESCE(AT_EXT_MOV_TO_AGY.DESCRIPTION, AT_TO_CITY_CODES.DESCRIPTION, AT_TO_ADDRESS_ATTENDED.STREET || ' ' || AT_TO_ADDRESS_ATTENDED.LOCALITY || ' ' || AT_TO_ADDRESS_ATTENDED.POSTAL_CODE) AS TO_ FROM OMS_OWNER.OFFENDERS AT_OFFENDER, OMS_OWNER.REFERENCE_CODES AT_MOV_TYPE_DESC, OMS_OWNER.REFERENCE_CODES AT_FROM_CITY_CODES, OMS_OWNER.REFERENCE_CODES AT_TO_CITY_CODES, OMS_OWNER.OFFENDER_BOOKINGS AT_OFFENDER_BOOKING, OMS_OWNER.AGENCY_LOCATIONS AT_EXT_MOV_FROM_AGY, OMS_OWNER.AGENCY_LOCATIONS AT_EXT_MOV_TO_AGY, OMS_OWNER.AGENCY_LOCATIONS AT_MOV_LOC_TO_OFFND_LOCTION, OMS_OWNER.OFFENDER_EXTERNAL_MOVEMENTS AT_OFFENDER_EXTERNAL_MOVEMENTS, OMS_OWNER.MOVEMENT_REASONS AT_MOVEMENT_REASONS, OMS_OWNER.OFFENDER_IDENTIFIERS AT_OFFENDER_PORT_REF, OMS_OWNER.OFFENDER_IDENTIFIERS AT_OFFENDER_CID, OMS_OWNER.OFFENDER_IDENTIFIERS AT_OFFENDER_HOREF, ADDRESSES AT_FROM_ADDRESS_ATTENDED, ADDRESSES AT_TO_ADDRESS_ATTENDED WHERE (AT_OFFENDER.OFFENDER_ID = AT_OFFENDER_BOOKING.OFFENDER_ID) AND (AT_OFFENDER_BOOKING.OFFENDER_BOOK_ID = AT_OFFENDER_EXTERNAL_MOVEMENTS.OFFENDER_BOOK_ID (+)) AND (AT_OFFENDER_EXTERNAL_MOVEMENTS.MOVEMENT_TYPE = AT_MOVEMENT_REASONS.MOVEMENT_TYPE AND AT_OFFENDER_EXTERNAL_MOVEMENTS.MOVEMENT_REASON_CODE = AT_MOVEMENT_REASONS.MOVEMENT_REASON_CODE) AND (AT_MOV_TYPE_DESC.CODE (+) = AT_OFFENDER_EXTERNAL_MOVEMENTS.MOVEMENT_TYPE) AND (AT_MOV_TYPE_DESC.DOMAIN (+) = ('MOVE_TYPE')) AND (AT_OFFENDER_EXTERNAL_MOVEMENTS.TO_AGY_LOC_ID = AT_MOV_LOC_TO_OFFND_LOCTION.AGY_LOC_ID (+)) AND (AT_MOV_LOC_TO_OFFND_LOCTION.AGENCY_LOCATION_TYPE = 'INST') AND (AT_OFFENDER_EXTERNAL_MOVEMENTS.TO_AGY_LOC_ID = AT_EXT_MOV_TO_AGY.AGY_LOC_ID (+)) AND (AT_OFFENDER_EXTERNAL_MOVEMENTS.FROM_AGY_LOC_ID = AT_EXT_MOV_FROM_AGY.AGY_LOC_ID (+)) AND (AT_OFFENDER_EXTERNAL_MOVEMENTS.TO_CITY = AT_TO_CITY_CODES.CODE (+)) AND (AT_TO_CITY_CODES.DOMAIN = 'CITY' OR AT_TO_CITY_CODES.DOMAIN IS NULL) AND (AT_OFFENDER_EXTERNAL_MOVEMENTS.FROM_CITY = AT_FROM_CITY_CODES.CODE (+)) AND (AT_FROM_CITY_CODES.DOMAIN = 'CITY' OR AT_FROM_CITY_CODES.DOMAIN IS NULL) AND (AT_OFFENDER_EXTERNAL_MOVEMENTS.TO_ADDRESS_ID = AT_TO_ADDRESS_ATTENDED.ADDRESS_ID (+)) AND (AT_OFFENDER_EXTERNAL_MOVEMENTS.FROM_ADDRESS_ID = AT_FROM_ADDRESS_ATTENDED.ADDRESS_ID (+)) AND (AT_OFFENDER_CID.OFFENDER_ID (+) = AT_OFFENDER_BOOKING.OFFENDER_ID) AND (AT_OFFENDER_CID.IDENTIFIER_TYPE (+) = 'CID') AND (AT_OFFENDER_PORT_REF.OFFENDER_ID (+) = AT_OFFENDER_BOOKING.OFFENDER_ID) AND (AT_OFFENDER_PORT_REF.IDENTIFIER_TYPE (+) = 'PORT REF') AND (AT_OFFENDER_HOREF.OFFENDER_ID (+) = AT_OFFENDER_BOOKING.OFFENDER_ID) AND (AT_OFFENDER_HOREF.IDENTIFIER_TYPE (+) = 'HOREF') AND (TRUNC(AT_OFFENDER_EXTERNAL_MOVEMENTS.MOVEMENT_DATE) = TO_DATE('22-11-2022 00:00:00', 'DD-MM-YYYY HH24:MI:SS') AND TRUNC(AT_OFFENDER_EXTERNAL_MOVEMENTS.MOVEMENT_DATE) <= TRUNC(CURRENT_TIMESTAMP) AND AT_MOV_LOC_TO_OFFND_LOCTION.AGY_LOC_ID = 'MHI' AND AT_OFFENDER_EXTERNAL_MOVEMENTS.DIRECTION_CODE = 'IN') UNION SELECT DISTINCT AT_OFFENDER.OFFENDER_ID_DISPLAY AS OFFENDER_ID_DISPLAY, AT_OFFENDER.LAST_NAME AS LAST_NAME, AT_OFFENDER_PORT_REF.IDENTIFIER AS IDENTIFIER, AT_OFFENDER_CID.IDENTIFIER AS CID, AT_OFFENDER_HOREF.IDENTIFIER AS HOREF, AT_OFFENDER.FIRST_NAME AS FIRST_NAME, AT_OFFENDER.BIRTH_DATE AS BIRTH_DATE, AT_OFFENDER_EXTERNAL_MOVEMENTS.MOVEMENT_DATE AS MOVEMENT_DATE, AT_OFFENDER_EXTERNAL_MOVEMENTS.MOVEMENT_TIME AS MOVEMENT_TIME, AT_MOV_TYPE_DESC.CODE AS CODE, AT_MOVEMENT_REASONS.DESCRIPTION AS DESCRIPTION, AT_OFFENDER_EXTERNAL_MOVEMENTS.DIRECTION_CODE AS DIRECTION_CODE, COALESCE(AT_EXT_MOV_FROM_AGY.DESCRIPTION, AT_FROM_CITY_CODES.DESCRIPTION, AT_FROM_ADDRESS_ATTENDED.STREET || ' ' || AT_FROM_ADDRESS_ATTENDED.LOCALITY || ' ' || AT_FROM_ADDRESS_ATTENDED.POSTAL_CODE) AS FROM_, COALESCE(AT_EXT_MOV_TO_AGY.DESCRIPTION, AT_TO_CITY_CODES.DESCRIPTION, AT_TO_ADDRESS_ATTENDED.STREET || ' ' || AT_TO_ADDRESS_ATTENDED.LOCALITY || ' ' || AT_TO_ADDRESS_ATTENDED.POSTAL_CODE) AS TO_ FROM OMS_OWNER.OFFENDERS AT_OFFENDER, OMS_OWNER.REFERENCE_CODES AT_MOV_TYPE_DESC, OMS_OWNER.REFERENCE_CODES AT_FROM_CITY_CODES, OMS_OWNER.REFERENCE_CODES AT_TO_CITY_CODES, OMS_OWNER.OFFENDER_BOOKINGS AT_OFFENDER_BOOKING, OMS_OWNER.AGENCY_LOCATIONS AT_EXT_MOV_FROM_AGY, OMS_OWNER.AGENCY_LOCATIONS AT_EXT_MOV_TO_AGY, OMS_OWNER.AGENCY_LOCATIONS AT_MOV_LOC_FROM_OFFND_LOCTION, OMS_OWNER.OFFENDER_EXTERNAL_MOVEMENTS AT_OFFENDER_EXTERNAL_MOVEMENTS, OMS_OWNER.MOVEMENT_REASONS AT_MOVEMENT_REASONS, OMS_OWNER.OFFENDER_IDENTIFIERS AT_OFFENDER_PORT_REF, OMS_OWNER.OFFENDER_IDENTIFIERS AT_OFFENDER_CID, OMS_OWNER.OFFENDER_IDENTIFIERS AT_OFFENDER_HOREF, ADDRESSES AT_FROM_ADDRESS_ATTENDED, ADDRESSES AT_TO_ADDRESS_ATTENDED WHERE (AT_OFFENDER.OFFENDER_ID = AT_OFFENDER_BOOKING.OFFENDER_ID) AND (AT_OFFENDER_BOOKING.OFFENDER_BOOK_ID = AT_OFFENDER_EXTERNAL_MOVEMENTS.OFFENDER_BOOK_ID (+)) AND (AT_OFFENDER_EXTERNAL_MOVEMENTS.MOVEMENT_TYPE = AT_MOVEMENT_REASONS.MOVEMENT_TYPE AND AT_OFFENDER_EXTERNAL_MOVEMENTS.MOVEMENT_REASON_CODE = AT_MOVEMENT_REASONS.MOVEMENT_REASON_CODE) AND (AT_MOV_TYPE_DESC.CODE (+) = AT_OFFENDER_EXTERNAL_MOVEMENTS.MOVEMENT_TYPE) AND (AT_MOV_TYPE_DESC.DOMAIN (+) = ('MOVE_TYPE')) AND (AT_OFFENDER_EXTERNAL_MOVEMENTS.FROM_AGY_LOC_ID = AT_MOV_LOC_FROM_OFFND_LOCTION.AGY_LOC_ID (+)) AND (AT_MOV_LOC_FROM_OFFND_LOCTION.AGENCY_LOCATION_TYPE = 'INST') AND (AT_OFFENDER_EXTERNAL_MOVEMENTS.TO_AGY_LOC_ID = AT_EXT_MOV_TO_AGY.AGY_LOC_ID (+)) AND (AT_OFFENDER_EXTERNAL_MOVEMENTS.FROM_AGY_LOC_ID = AT_EXT_MOV_FROM_AGY.AGY_LOC_ID (+)) AND (AT_OFFENDER_EXTERNAL_MOVEMENTS.TO_CITY = AT_TO_CITY_CODES.CODE (+)) AND (AT_TO_CITY_CODES.DOMAIN = 'CITY' OR AT_TO_CITY_CODES.DOMAIN IS NULL) AND (AT_OFFENDER_EXTERNAL_MOVEMENTS.FROM_CITY = AT_FROM_CITY_CODES.CODE (+)) AND (AT_FROM_CITY_CODES.DOMAIN = 'CITY' OR AT_FROM_CITY_CODES.DOMAIN IS NULL) AND (AT_OFFENDER_EXTERNAL_MOVEMENTS.TO_ADDRESS_ID = AT_TO_ADDRESS_ATTENDED.ADDRESS_ID (+)) AND (AT_OFFENDER_EXTERNAL_MOVEMENTS.FROM_ADDRESS_ID = AT_FROM_ADDRESS_ATTENDED.ADDRESS_ID (+)) AND (AT_OFFENDER_CID.OFFENDER_ID (+) = AT_OFFENDER_BOOKING.OFFENDER_ID) AND (AT_OFFENDER_CID.IDENTIFIER_TYPE (+) = 'CID') AND (AT_OFFENDER_PORT_REF.OFFENDER_ID (+) = AT_OFFENDER_BOOKING.OFFENDER_ID) AND (AT_OFFENDER_PORT_REF.IDENTIFIER_TYPE (+) = 'PORT REF') AND (AT_OFFENDER_HOREF.OFFENDER_ID (+) = AT_OFFENDER_BOOKING.OFFENDER_ID) AND (AT_OFFENDER_HOREF.IDENTIFIER_TYPE (+) = 'HOREF') AND (TRUNC(AT_OFFENDER_EXTERNAL_MOVEMENTS.MOVEMENT_DATE) = TO_DATE('22-11-2022 00:00:00', 'DD-MM-YYYY HH24:MI:SS') AND TRUNC(AT_OFFENDER_EXTERNAL_MOVEMENTS.MOVEMENT_DATE) <= TRUNC(CURRENT_TIMESTAMP) AND AT_MOV_LOC_FROM_OFFND_LOCTION.AGY_LOC_ID = 'MHI' AND AT_OFFENDER_EXTERNAL_MOVEMENTS.DIRECTION_CODE = 'OUT')), dataset_ AS (SELECT OFFENDER_ID_DISPLAY, LAST_NAME, IDENTIFIER, CID, HOREF, FIRST_NAME, BIRTH_DATE, MOVEMENT_DATE, MOVEMENT_TIME, CODE, DESCRIPTION, DIRECTION_CODE, FROM_, TO_, CASE WHEN CODE = 'TAP' THEN 'Temporary Absence / Home Leave' ELSE CASE WHEN CODE = 'ADM' THEN 'Admission' ELSE CASE WHEN CODE = 'REL' THEN 'Release' ELSE CASE WHEN CODE = 'CRT' THEN 'Court' ELSE CASE WHEN CODE = 'TRN' THEN 'Transfer' END END END END END AS V_MOVEMENTTYEDESCRIPTION FROM dataset_base_)",
