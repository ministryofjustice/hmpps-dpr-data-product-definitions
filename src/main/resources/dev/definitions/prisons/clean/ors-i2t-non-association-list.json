      "description": "OR - Demographics",
      "datasource": "nomis",
      "query": "dataset_base_ AS (SELECT DISTINCT AT_OFFENDER.OFFENDER_ID_DISPLAY AS OFFENDER_ID_DISPLAY, AT_OFFENDER.LAST_NAME AS LAST_NAME, AT_OFFENDER.FIRST_NAME AS FIRST_NAME, AT_OFFENDERS_NA.OFFENDER_ID_DISPLAY AS NA_NOMS_NUMBER, AT_OFFENDERS_NA.LAST_NAME AS NA_LAST_NAME, AT_OFFENDERS_NA.FIRST_NAME AS NA_FIRST_NAME, AT_OFFENDER_NA_DETAILS.NS_TYPE AS NS_TYPE, AT_OFFENDER_NA_DETAILS.NS_REASON_CODE AS NS_REASON_CODE, AT_OFFENDER_NON_ASSOCIATIONS.RECIP_NS_REASON_CODE AS RECIP_NS_REASON_CODE, AT_OFFENDER_NA_DETAILS.NS_EFFECTIVE_DATE AS NS_EFFECTIVE_DATE, AT_OFFENDER_NA_DETAILS.NS_EXPIRY_DATE AS NS_EXPIRY_DATE, AT_OFFENDER_NA_DETAILS.COMMENT_TEXT AS COMMENT_TEXT, AT_OFFENDER_NA_DETAILS.AUTHORIZED_STAFF AS AUTHORIZED_STAFF, AT_OFFENDER_BOOKING_NA.ACTIVE_FLAG AS ACTIVE_FLAG FROM OMS_OWNER.OFFENDERS AT_OFFENDER, OMS_OWNER.OFFENDER_BOOKINGS AT_OFFENDER_BOOKING, OMS_OWNER.AGENCY_LOCATIONS AT_OFFENDERS_LOCATIONS, OMS_OWNER.CASELOAD_AGENCY_LOCATIONS AT_CASELOAD_LOCATIONS, OMS_OWNER.OFFENDERS AT_OFFENDERS_NA, OMS_OWNER.OFFENDER_BOOKINGS AT_OFFENDER_BOOKING_NA, OMS_OWNER.OFFENDER_NA_DETAILS AT_OFFENDER_NA_DETAILS, OMS_OWNER.OFFENDER_NON_ASSOCIATIONS AT_OFFENDER_NON_ASSOCIATIONS WHERE (AT_OFFENDER.OFFENDER_ID = AT_OFFENDER_BOOKING.OFFENDER_ID) AND (AT_OFFENDERS_LOCATIONS.AGENCY_LOCATION_TYPE = 'INST') AND (AT_CASELOAD_LOCATIONS.AGY_LOC_ID = AT_OFFENDER_BOOKING.AGY_LOC_ID) AND (AT_CASELOAD_LOCATIONS.AGY_LOC_ID = AT_OFFENDERS_LOCATIONS.AGY_LOC_ID (+)) AND (NOT AT_CASELOAD_LOCATIONS.AGY_LOC_ID IN ('OUT', 'TRN') AND NOT AT_CASELOAD_LOCATIONS.CASELOAD_ID IN ('ADMINC', 'CADM_I', 'MULTI')) AND (AT_OFFENDER_NON_ASSOCIATIONS.OFFENDER_ID (+) = AT_OFFENDER.ROOT_OFFENDER_ID) AND (AT_OFFENDERS_NA.ROOT_OFFENDER_ID = AT_OFFENDER_NA_DETAILS.NS_OFFENDER_ID) AND (AT_OFFENDER_NA_DETAILS.OFFENDER_ID = AT_OFFENDER_NON_ASSOCIATIONS.OFFENDER_ID AND AT_OFFENDER_NA_DETAILS.NS_OFFENDER_ID = AT_OFFENDER_NON_ASSOCIATIONS.NS_OFFENDER_ID) AND (AT_OFFENDERS_NA.OFFENDER_ID = AT_OFFENDER_BOOKING_NA.OFFENDER_ID) AND ((AT_OFFENDERS_LOCATIONS.AGY_LOC_ID IN (SELECT ESTABLISHMENT_CODE FROM prompt_ /* = #Prompt('Establishment Code (All for all)','A','Establishment\\Establishment Code',multi,free,,Not_Persistent,,User:3) */) OR 'All' IN (SELECT ESTABLISHMENT_CODE FROM prompt_ /* = #Prompt('Establishment Code (All for all)','A','Establishment\\Establishment Code',multi,free,,Not_Persistent,,User:3) */)) AND (AT_OFFENDER_BOOKING.ACTIVE_FLAG = 'Y'))), dataset_ AS (SELECT OFFENDER_ID_DISPLAY, LAST_NAME, FIRST_NAME, NA_NOMS_NUMBER, NA_LAST_NAME, NA_FIRST_NAME, NS_TYPE, NS_REASON_CODE, RECIP_NS_REASON_CODE, NS_EFFECTIVE_DATE, NS_EXPIRY_DATE, COMMENT_TEXT, AUTHORIZED_STAFF, ACTIVE_FLAG FROM dataset_base_)",
