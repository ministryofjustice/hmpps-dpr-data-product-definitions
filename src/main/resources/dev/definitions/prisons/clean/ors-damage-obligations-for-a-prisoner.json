      "query": "dataset_base_ AS (SELECT AT_OFFENDER.OFFENDER_ID_DISPLAY AS OFFENDER_ID_DISPLAY, AT_OFFENDER.LAST_NAME AS LAST_NAME, AT_OFFENDER.FIRST_NAME AS FIRST_NAME, AT_OFF_DAMAGE_OBL.OFFENDER_ID AS OFFENDER_ID, AT_OFF_DAMAGE_OBL.OFFENDER_DMG_OBLIGATION_ID AS OFFENDER_DMG_OBLIGATION_ID, AT_OFF_DAMAGE_OBL.REFERENCE_NO AS REFERENCE_NO, AT_OFF_DAMAGE_OBL.START_DATE AS START_DATE, AT_OFF_DAMAGE_OBL.END_DATE AS END_DATE, AT_OFF_DAMAGE_OBL.OBLIGATION_STATUS AS OBLIGATION_STATUS, AT_DAMOBL_CASELOAD.DESCRIPTION AS DESCRIPTION, AT_OFF_DAMAGE_OBL.AMOUNT_TO_PAY AS AMOUNT_TO_PAY, AT_OFF_DAMAGE_OBL.PAID_AMOUNT AS PAID_AMOUNT FROM OMS_OWNER.OFFENDERS AT_OFFENDER, OMS_OWNER.OFFENDER_DAMAGE_OBLIGATIONS AT_OFF_DAMAGE_OBL, CASELOADS AT_DAMOBL_CASELOAD WHERE (AT_DAMOBL_CASELOAD.CASELOAD_ID = AT_OFF_DAMAGE_OBL.AGY_LOC_ID) AND (AT_OFF_DAMAGE_OBL.OFFENDER_ID = AT_OFFENDER.OFFENDER_ID) AND (AT_DAMOBL_CASELOAD.CASELOAD_TYPE = 'INST') AND (AT_DAMOBL_CASELOAD.ACTIVE_FLAG = 'Y') AND (AT_DAMOBL_CASELOAD.TRUST_ACCOUNTS_FLAG = 'Y') AND (AT_OFFENDER.OFFENDER_ID_DISPLAY = (SELECT PRISONER_NUMBER FROM prompt_ /* = #Prompt('NOMS Number','A','Offender\\NOMS Number',mono,free,,Not_Persistent,,User:1) */))), dataset_ AS (SELECT OFFENDER_ID_DISPLAY, LAST_NAME, FIRST_NAME, OFFENDER_ID, OFFENDER_DMG_OBLIGATION_ID, REFERENCE_NO, START_DATE, END_DATE, OBLIGATION_STATUS, DESCRIPTION, AMOUNT_TO_PAY, PAID_AMOUNT FROM dataset_base_)",
