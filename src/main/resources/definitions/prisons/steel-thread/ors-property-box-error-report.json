      "name": "Prop Box Recycling",
      "description": "Demographic",
      "datasource": "nomis",
      "query": "dataset_base_ AS (SELECT DT_PROPERTY_BOXES.SEAL_MARK AS SEAL_MARK, AT_PROP_LOC.DESCRIPTION AS DESCRIPTION, DT_PROPERTY_BOXES.PROPERTY_CONTAINER_ID AS PROPERTY_CONTAINER_ID, AT_OFFENDER.OFFENDER_ID_DISPLAY AS OFFENDER_ID_DISPLAY, AT_OFFENDER.LAST_NAME AS LAST_NAME, AT_HEADER_BLOCK.STATUS_DISPLAY || ' ' || AT_HEADER_BLOCK.IN_OUT_STATUS AS COMMUNITY_STATUS FROM OMS_OWNER.OFFENDERS AT_OFFENDER, OMS_OWNER.OFFENDER_BOOKINGS AT_OFFENDER_BOOKING, OMS_OWNER.V_HEADER_BLOCK AT_HEADER_BLOCK, OMS_OWNER.AGENCY_INTERNAL_LOCATIONS AT_PROP_LOC, (SELECT Offender_Book_ID, Property_Container_ID, Active_Flag, Seal_Mark, Agy_Loc_ID, Expiry_Date, Internal_Location_ID, PROPOSED_DISPOSAL_DATE, Container_Code, Property_Only_Flag, Comment_Text FROM oms_owner.offender_ppty_containers WHERE active_flag = 'Y' AND Agy_Loc_ID = 'CHI') DT_PROPERTY_BOXES WHERE (AT_OFFENDER.OFFENDER_ID = AT_OFFENDER_BOOKING.OFFENDER_ID) AND (AT_OFFENDER_BOOKING.OFFENDER_BOOK_ID = DT_PROPERTY_BOXES.OFFENDER_BOOK_ID (+)) AND (AT_OFFENDER_BOOKING.OFFENDER_BOOK_ID = AT_HEADER_BLOCK.OFFENDER_BOOK_ID (+)) AND (DT_PROPERTY_BOXES.INTERNAL_LOCATION_ID = AT_PROP_LOC.INTERNAL_LOCATION_ID (+)) AND DT_PROPERTY_BOXES.ACTIVE_FLAG = 'Y'), dataset_ AS (SELECT SEAL_MARK, DESCRIPTION, PROPERTY_CONTAINER_ID, OFFENDER_ID_DISPLAY, LAST_NAME, COMMUNITY_STATUS FROM dataset_base_)",
