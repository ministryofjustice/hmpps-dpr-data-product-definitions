  "dataset": [
    {
      "id": "3377299/DP10",
      "name": "Prisoners With Property",
      "description": "OR - Demographics",
      "datasource": "nomis",
      "query": "dataset_base_ AS (SELECT DISTINCT DT_PROPERTY_BOXES.PROPERTY_CONTAINER_ID AS PROPERTY_CONTAINER_ID, DT_PROPERTY_BOXES.SEAL_MARK AS SEAL_MARK, DT_PROPERTY_BOXES.CONTAINER_CODE AS CONTAINER_CODE, AT_PROP_LOC.DESCRIPTION AS DESCRIPTION, DT_PROPERTY_BOXES.EXPIRY_DATE AS EXPIRY_DATE, DT_PROPERTY_BOXES.PROPOSED_DISPOSAL_DATE AS PROPOSED_DISPOSAL_DATE, AT_OFFENDER.OFFENDER_ID_DISPLAY AS OFFENDER_ID_DISPLAY, AT_OFFENDER.LAST_NAME AS LAST_NAME, AT_OFFENDER.FIRST_NAME AS FIRST_NAME, SUBSTR(AT_AGENCY_INTERNAL_LOCATION_L4.DESCRIPTION, LENGTH(AT_AGENCY_INTERNAL_LOCATION_L4.AGY_LOC_ID) + 2, LENGTH(AT_AGENCY_INTERNAL_LOCATION_L4.DESCRIPTION)) AS UNIT_DESCRIPTION_4_SHORT FROM OMS_OWNER.OFFENDERS AT_OFFENDER, OMS_OWNER.AGENCY_INTERNAL_LOCATIONS AT_AGENCY_INTERNAL_LOCATION_L4, OMS_OWNER.OFFENDER_BOOKINGS AT_OFFENDER_BOOKING, OMS_OWNER.AGENCY_LOCATIONS AT_WING_ESTABLISHMENT, OMS_OWNER.AGENCY_INTERNAL_LOCATIONS AT_PROP_LOC, (SELECT Offender_Book_ID, Property_Container_ID, Active_Flag, Seal_Mark, Agy_Loc_ID, Expiry_Date, Internal_Location_ID, PROPOSED_DISPOSAL_DATE, Container_Code, Property_Only_Flag, Comment_Text FROM oms_owner.offender_ppty_containers WHERE active_flag = 'Y' AND Agy_Loc_ID = 'BFI') DT_PROPERTY_BOXES, USER_ACCESSIBLE_CASELOADS WING_CASELOAD_SECURITY WHERE (AT_OFFENDER.OFFENDER_ID = AT_OFFENDER_BOOKING.OFFENDER_ID) AND (AT_OFFENDER_BOOKING.LIVING_UNIT_ID = AT_AGENCY_INTERNAL_LOCATION_L4.INTERNAL_LOCATION_ID) AND (AT_AGENCY_INTERNAL_LOCATION_L4.AGY_LOC_ID = AT_WING_ESTABLISHMENT.AGY_LOC_ID) AND (AT_WING_ESTABLISHMENT.AGY_LOC_ID = WING_CASELOAD_SECURITY.CASELOAD_ID) AND (AT_OFFENDER_BOOKING.OFFENDER_BOOK_ID = DT_PROPERTY_BOXES.OFFENDER_BOOK_ID (+)) AND (DT_PROPERTY_BOXES.INTERNAL_LOCATION_ID = AT_PROP_LOC.INTERNAL_LOCATION_ID (+)) AND (WING_CASELOAD_SECURITY.USERNAME = UPPER('SQZ39G')) AND ((AT_OFFENDER_BOOKING.ACTIVE_FLAG = 'Y') AND NOT DT_PROPERTY_BOXES.CONTAINER_CODE IS NULL)), dataset_ AS (SELECT PROPERTY_CONTAINER_ID, SEAL_MARK, CONTAINER_CODE, DESCRIPTION, EXPIRY_DATE, PROPOSED_DISPOSAL_DATE, OFFENDER_ID_DISPLAY, LAST_NAME, FIRST_NAME, UNIT_DESCRIPTION_4_SHORT FROM dataset_base_)",
      "parameters": [],
